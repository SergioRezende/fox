  CLOSE DATA
   
   LCCBR_PK = -1


   SELECT distinct;
     CB.LOJA;
    ,EMP.SIGLA;
    ,FCLCCBR_PK()+LCCBR_PK      AS PK;
    ,LCCBR_PK                   AS PK_INI;
    ,000                          AS UNEM_PK;
    ,""                         AS LOGO;
    ,"BANCO"                    AS TP_LANC;
    ,CHRTRAN(STR(CB.LOJA,3)+STR(CB.BANCO,3)," ","0")+" "  AS CODIGO ;
    , EMP.SIGLA+"-"+;
      CHRTRAN(STR(CB.BANCO,3)," ","0")+;
      +"-"+ALLTRIM(CB.BANCO_NOME)+"-[ "+RTRIM(OPERACAO)+" ]-"+ALLTRIM(CB.CONVENIO)  AS DESCRICAO;
    ,CB.BANCO;
    ,CB.AGENCIA;
    ,CB.AGENCIA_DG;
    ,CB.CONTA;
    ,CB.CONTA_DG;
    ,CB.CONVENIO;
    ,CB.CARTEIRA;
    ,CB.VARIACAO;
    ,CB.BANCO_NOME;
    ,CB.CODCEDENTE;
    ,CB.COD_EMPRES;
    ,CB.NOME_EMP;
    ,CB.TP_INSCR;
    ,CB.NUM_INSCR;
    ,""     AS PATHREMES;
    ,""     AS PATHRETOR;
    ,0      AS NRO_REMES;
    ,"01"             AS COD_SRVC;
    ,"COBRANCA"       AS LIT_SRVC;
    ,""     AS FRM_PGTO;
    ,""     AS CRT_ADM;
    ,""     AS CRT_REDE;
    ,""     AS CRT_DB_CR;
    ,""     AS CRT_CNPJ;
    ,"Sim"  AS FIXA_DMAIS;
    ,0      AS DMAIS;
    ,"Sim"  AS BLT_GERAR; 
    ,""     AS BLT_MODL;
    ,CB.BLT_SEQINI;
    ,CB.BLT_SEQATU+1 AS BLT_SEQPRX;
    ,CB.BLT_SEQFIM;
    ,0      AS PRX_AVISO;
    ,CB.IDENT_SIS;
    ,CB.INSTRUC_A;
    ,CB.INSTRUC_B;
    ,"Sim"  AS CB_RGSTR;
    ,400    AS TMNH_ARQ;
    ,30     AS DIAS_LMT;
    ,""     AS CTA_CONTAB;
    ,"PAGAVEL EM QUALQUER BANCO ATE O VENCIMENTO" AS INST_LCPG;
    ,"" AS INST_DSCT;
    ,"" AS INST_CMPS;
    ,0  AS TX_MULTA;
    ,0  AS TX_MORA;
    ,0  AS TX_COBRA;
    ,0  AS IND_TX_CBR;
    ,0  AS VLR_CBR;
    ,1  AS IND_VL_CBR;
    ,0  AS SALDO;
    ,"CARGA SISTEMA LEGADO"   AS LOG;
   FROM ;
        X:\SCGC\cobranc\COBRANCA CB ;
     ,  X:\SCGC\CENTRAL\EMPRESA EMP ;
  WHERE ;
     CB.LOJA = EMP.EMPRESA  ;
  INTO TABLE X:\PRODUCAO\LCCBR ;
  ORDER BY ;
    CB.LOJA;
    ,CB.BANCO



   CLOSE DATA


  USE  X:\PRODUCAO\LCCBR
    
  GO TOP
  CTR = 0
  CODANT = CHRTRAN(STR(LOJA,3)+STR(BANCO,3)," ","0")
  SKIP

  DO WHILE !EOF()
     IF CODANT = CHRTRAN(STR(LOJA,3)+STR(BANCO,3)," ","0")
        CTR = CTR + 1
        REPLACE CODIGO WITH CHRTRAN(STR(LOJA,3)+STR(BANCO,3)," ","0")+STR(CTR,1) 
     ELSE
        CTR = 0
        REPLACE CODIGO WITH CHRTRAN(STR(LOJA,3)+STR(BANCO,3)," ","0") 
     ENDIF
     CODANT = CHRTRAN(STR(LOJA,3)+STR(BANCO,3)," ","0")
     SKIP
  ENDDO   
  GO TOP 

  REPLACE ALL UNEM_PK WITH  13 FOR ALLTRIM(SIGLA) = "GYN"
  GO TOP 


  REPLACE ALL UNEM_PK WITH  14 FOR ALLTRIM(SIGLA) = "SIA"
  GO TOP 
  
  REPLACE ALL UNEM_PK WITH  15 FOR ALLTRIM(SIGLA) = "W-3"
  GO TOP 

  REPLACE ALL UNEM_PK WITH  16 FOR ALLTRIM(SIGLA) = "BGS"
  GO TOP 
  
  REPLACE ALL UNEM_PK WITH  17 FOR ALLTRIM(SIGLA) = "IPG"
  GO TOP 
  
  REPLACE ALL UNEM_PK WITH  18 FOR ALLTRIM(SIGLA) = "CGB"
  GO TOP 
  
  REPLACE ALL UNEM_PK WITH  19 FOR ALLTRIM(SIGLA) = "ARG"
  GO TOP 
  
  REPLACE ALL UNEM_PK WITH  20 FOR ALLTRIM(SIGLA) = "IDP"
  GO TOP 
  

  REPLACE ALL UNEM_PK WITH  21 FOR ALLTRIM(SIGLA) = "T63"
  GO TOP 
  
  
  REPLACE ALL UNEM_PK WITH  22 FOR ALLTRIM(SIGLA) = "ANP"
  GO TOP 
  
  REPLACE ALL UNEM_PK WITH  23 FOR ALLTRIM(SIGLA) = "DPO"
  GO TOP 
  
  REPLACE ALL UNEM_PK WITH  24 FOR ALLTRIM(SIGLA) = "ABO"
  GO TOP 
  
  REPLACE ALL UNEM_PK WITH  25 FOR ALLTRIM(SIGLA) = "IDL"
  GO TOP 

  REPLACE ALL UNEM_PK WITH  26 FOR ALLTRIM(SIGLA) = "PPP"
  GO TOP 
  
  

  
  


RETURN


FUNCTION FCLCCBR_PK

     LCCBR_PK = LCCBR_PK + 1

RETURN(0)



