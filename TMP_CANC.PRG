SET EXCL OFF
SET DELE ON
SET DATE GERMAN
SET CENT ON
SET TALK ON
SET EXCL OFF

SELECT ;
	NT.EMPRESA,;
	NT.NOTA,;
	NT.STATUS;
FROM ;
	Q:\SCGC\CENTRAL\NOTA NT,;
	Q:\SCGC\CENTRAL\DUPLICAT DP;
WHERE ;
        NT.EMPRESA = DP.EMPRESA;
   AND  NT.NOTA    = DP.NOTA;
   AND  NT.STATUS  = 2 ;
GROUP BY ;
	NT.EMPRESA,;
	NT.NOTA,;
	NT.STATUS;
   
   
   	
	
	
