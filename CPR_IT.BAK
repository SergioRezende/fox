
SELECT ;
   IT.EMPRESA      AS EMPRESA;
  ,EMP.SIGLA        AS SIGLA;
  ,NF.NOTA-3000000  AS NOTA;
  ,IT.ORDEM         AS SEQUENCIA;
  ,IT.CODIGO ;
  ,IT.QTDE ;
FROM ;
   Q:\SCGC\LOJA\ITEMMOV   IT;
 , Q:\SCGC\LOJA\NOTA      NF;
 , Q:\SCGC\LOJA\EMPRESA   EMP;
WHERE ;
      NF.EMPRESA = IT.EMPRESA;
  AND EMP.EMPRESA = IT.EMPRESA;
  AND;
	  (   IT.NOTA >3000000;
	  AND IT.NOTA <4000000;
	  ) ;  
  AND NF.NOTA = IT.NOTA;
  AND NF.DATA >= {01.10.2014} ;
  AND LEFT(NF.OPERACAO,1) = "S" ;
ORDER BY  ;
   IT.EMPRESA ;
  ,EMP.SIGLA;
  ,NF.DATA;
  ,NF.NOTA;
INTO TABLE ;
  X:\TMP\IT_FOX  

 
  
  
  
  