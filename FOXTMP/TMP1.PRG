SET TALK ON
CLOSE ALL
USE \SCGC\COMUM\GRFISCAL EXCL
SET ORDER TO TAG TRIBUTACAO OF Q:\SCGC\COMUM\GRFISCAL.CDX

SET STEP ON
SELE 0
USE \SCGC\CENTRAL\GRUPO
SET ORDER TO TAG SUBGRUPOS OF Q:\SCGC\CENTRAL\GRUPO.CDX
GO TOP
DO WHILE !EOF()
	SELE GRFISCAL
	IF LEN(ALLTRIM(GRUPO.CLASSIFICA)) = 5 AND GRUPO.CDG_TIPO = 2

		APPEND BLANK
		REPLACE UF WITH "DF"
		REPLACE SUBGRUPO WITH LEFT(GRUPO.CLASSIFICA,5)		

		APPEND BLANK
		REPLACE UF WITH "MT"
		REPLACE SUBGRUPO WITH LEFT(GRUPO.CLASSIFICA,5)		

		APPEND BLANK
		REPLACE UF WITH "TO"
		REPLACE SUBGRUPO WITH LEFT(GRUPO.CLASSIFICA,5)		

		APPEND BLANK
		REPLACE UF WITH "GO"
		REPLACE SUBGRUPO WITH LEFT(GRUPO.CLASSIFICA,5)		

	ENDIF
	SELE GRUPO
	SKIP
ENDDO
