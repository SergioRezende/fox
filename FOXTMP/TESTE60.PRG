USE D:\SCGC2003\CENTRAL\NOTA
SET ORDER TO TAG DATA OF D:\SCGC2003\CENTRAL\NOTA.CDX
SEEK STR(5,3)+DTOS({01.01.2003})

COPY  FIELDS   EMPRESA,NOTA,SERIE,DATA,HORA,STATUS,NATU_OPER,OPERACAO,;
			TIPO,FAVORECIDO,NOME,ENDERECO,BAIRRO,CIDADE,;
			UF,CEP,FONE,REVENDEDOR,NATU_CLI,TP_PESSOA,TP_INSCR,;
			INSCRICAO,INSCSUBST,INSC_MUNI,CFO,;
			NATUREZA,COD_CITA,CITACAO,;
			CITACAO1,CUPOM,TOTSERVICO,BASE_ISS,;
			ALIQ_ISS,VLR_ISS,BASE_ICMS,ALIQ_ICMS,;
			VLR_ICMS,BASE_SBBRT,BASE_SUBS,ICMS_SUBS,;
			BASE_ISENT,BASE_OUTR,BASE_ISIPI,;
			BASE_IPI,VLR_IPI,TOTPRODUTO,;
			VLRFRETE,VLRSEGURO,VLRDESPES,;
			TOTAL_NOTA,INSC_EMIT,CGC_EMIT,;
			TP_TRANSP,TRANSP,PLACA_TRAN,UF_TRAN,;
			QTDE_VOL,ESPEC_VOL,NUMERO,;
			MARCA,PES_BRT,PES_LIQ,PGTO_FRETE;
        TO Y:\LIXO\SERGIO\NFTO2003 FOR  nota.uf = "TO" ;
					   AND nota.operacao = "S" ;
					   AND nota.status  = 1 ;
   WHILE  nota.empresa = 5 AND ;
	  nota.data >= {01.01.2003} AND nota.data < {01.01.2004}
***********************************************************

