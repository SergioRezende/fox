SELECT  DATA,OPERACAO,TIPO,NOTA,NATU_CLI,CODIGO,DESCONTO,QTDE,DESCRICAO ;
   FROM Q:\SCGC\LOJA\ITEMMOV IT;
   WHERE      LEFT(OPERACAO,1) = "S" ;
          AND DESCONTO > 35;
          AND    DATA > {01.01.2002};
          AND LEFT(IT.CLASSIFICA,2) $ "00/01/02";
   ORDER BY DESCONTO DESC, CODIGO DESC, DATA


SELECT  CODIGO,INT(DESCONTO) AS  DSCT,SUM(QTDE),DESCRICAO ;
   FROM Q:\SCGC\LOJA\ITEMMOV IT;
   WHERE      LEFT(OPERACAO,1) = "S" ;
          AND DESCONTO > 35;
          AND    DATA > {01.01.2002};
          AND LEFT(IT.CLASSIFICA,2) $ "00/01/02";
   ORDER BY DSCT DESC, CODIGO DESC;
   GROUP BY DSCT,CODIGO
   

SELECT  CODIGO,INT(DESCONTO) AS  DSCT,SUM(QTDE),DESCRICAO ;
   FROM Q:\SCGC\LOJA\ITEMMOV IT;
   WHERE      LEFT(OPERACAO,1) = "E" ;
		  AND TIPO   = "RBC" ;
          AND    DATA > {01.01.2002};
          AND LEFT(IT.CLASSIFICA,2) $ "00/01/02";
   ORDER BY DSCT DESC, CODIGO DESC;
   GROUP BY DSCT,CODIGO
