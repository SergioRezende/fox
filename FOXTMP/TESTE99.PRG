SELECT ;
   FR.CODIGO,;
   FR.NOME,;
   FR.ESTADO         	AS UF,;
   STR(FR.CEP,8)        AS CEP,;
   (FR.ENDERECO+FR.CIDADE) AS END;
  FROM ;
       Q:\scgc\CENTRAL\FORNECE FR, ;
       Q:\scgc\CENTRAL\NOTAENT NF ;
  WHERE ;
  	AND NF.DATA >= {01.01.2006} ;
  	AND NF.DATA <= {10.08.2006} ;
    AND LEFT(NF.SITUACAO,1) = "C" ;
    AND !(NF.CH_OPERA $ "5/6") ;
  	AND NF.CODFORN = FR.CODIGO;
  	AND EMPT(FR.CEP) ;
  GROUP BY NF.FAVORECIDO  ;
  ORDER BY NF.FAVORECIDO INTO CURSOR CUR_PART
