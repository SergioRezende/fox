SELECT EMP.SIGLA,EMP.nome AS EMPRESA ,;
       USR.USUARIO,USR.NOME AS VENDEDOR,;
       NF.NOME AS CLIENTE,SUM(TOTAL_NOTA) as VALOR ;
   FROM Q:\SCGC\COMUM\USUARIO USR, ;
        Q:\SCGC\LOJA\EMPRESA EMP, ;
        Q:\SCGC\LOJA\NOTA NF,;
        Q:\SCGC\LOJA\ITCOMISS CM;
   WHERE     NF.CH_OPERA = "1" ;
         AND NF.STATUS <> 2 ;
         AND EMP.EMPRESA = NF.EMPRESA;
         AND USR.USUARIO = CM.VENDEDOR;
