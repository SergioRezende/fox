SET EXCL OFF
SET DELE ON

SET DATE TO GERMAN
SET CENTU ON
SET ESCAPE ON
SET TALK ON
ON ERROR
ON KEY


CLOSE ALL

SELE 0
USE Q:\SCGC\COMUM\TIPOOPER
SET ORDER TO TAG TIPO

SELE 0
use q:\scgc\CENTRAL\NOTA
SET ORDER TO TAG DATA


SET NEAR ON

SEEK STR(10,3)+DTOS({01.01.2005})

do while !eof()

	if  EMPRESA <> 10
	     EXIT
	ENDIF
  

	if NOT NOTA.TIPO $ "VLB/VFB/VLP/VFW"
			skip
			loop
	endif
	

	SELE TIPOOPER
	DO CASE
		CASE NOTA.UF <> "GO"
			SEEK "S"+"VFW"
		CASE NOTA.tipo = "VLB"
			SEEK "S"+"VLP"
		CASE NOTA.TIPO = "VFB"
			SEEK "S"+"VFW"
		OTHERWISE
			SEEK "S"+NOTA.TIPO
	ENDCASE
	

  
  	SELE NOTA
  
    =RLOCK()

    REPLACE NOTA.CH_OPERA WITH TIPOOPER.CH_OPERA
    REPLACE NOTA.CH_PRODU WITH TIPOOPER.CH_PRODU
    REPLACE NOTA.CH_MOTIV WITH TIPOOPER.CH_MOTIV
    REPLACE NOTA.CH_DESTI WITH TIPOOPER.CH_DESTI
    REPLACE NOTA.CH_CONTR WITH TIPOOPER.CH_CONTR
    REPLACE NOTA.CH_CONDI WITH TIPOOPER.CH_CONDI
    REPLACE NOTA.TIPO     WITH TIPOOPER.TIPO
    REPLACE NOTA.CFO      WITH TIPOOPER.CFO


	skip
enddo

