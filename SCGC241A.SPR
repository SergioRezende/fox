*
FUNCTION _rml0rt6vi     &&  M.DESCT_P WHEN
#REGION 1
M.DESCT_P = 1
FOR I = 1 TO 6
	IS =STR(I,1)
	M.DESCT_P = DESCT_P * (1 - M.DESCT_&IS / 100)
NEXT
M.DESCT_P = (1 - M.DESCT_P) * 100
SHOW GET m.desct_p

RETURN(.F.)

