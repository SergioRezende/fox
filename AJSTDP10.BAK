DO DF
SET EXCL OFF
SET DELE ON
SET DATE GERMAN
SET CENTU ON

SELE 0
USE X:\SCGC\LOJA\DUPLICAT ALIAS DPLOJA
SET ORDER TO TAG DOC



SELE 0
USE X:\SCGC\CENTRAL\DUPLICAT ALIAS DPCENTRAL
SET ORDER TO TAG DOC

SELE DPLOJA
SET RELATION TO STR(EMPRESA,3)+STR(DUPLICATA,9) INTO DPCENTRAL
SET SKIP TO DPCENTRAL

BROWS FIELDS DPLOJA.EMPRESA   ,DPLOJA.DUPLICATA, DPLOJA.VLR_DOC,DPLOJA.VLR_ISSRTD,;
             DPCENTRAL.EMPRESA,DPCENTRAL.DUPLICATA, DPCENTRAL.VLR_DOC,DPCENTRAL.VLR_ISSRTD



REPLACE ALL ;
            DPCENTRAL.VLR_DOC    WITH DPLOJA.VLR_DOC,;
            DPCENTRAL.VLR_ISSRTD WITH DPLOJA.VLR_ISSRTD;
      FOR ;
            RLOCK() ;
            AND ;
            DPCENTRAL.VLR_DOC    <>  DPLOJA.VLR_DOC ;
            AND ;
            DPCENTRAL.VLR_ISSRTD <> DPLOJA.VLR_ISSRTD;
            AND ; 
            DPLOJA = 10 ;
            AND ;
            DPLOJA.DUPLICATA = DPCENTRAL.DUPLICATA;
            AND ;
            DPLOJA.EMPRESA   = DPCENTRAL.EMPRESA
           
      