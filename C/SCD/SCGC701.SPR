*
FUNCTION _2lx0oovs3     &&  LNvendedor VALID
#REGION 1
SELECT usuario
SET ORDER TO TAG usuario
IF LASTKEY() = 9
   ON KEY LABEL ESCAPE
   DO loc_dlog
   ON KEY LABEL ESCAPE KEYBOARD "{BACKTAB}"
   IF LASTKEY() = 27
		SELECT clientes
		RETURN .F.
   ENDIF
ELSE
	SET ORDER TO TAG usuario
	IF LNvendedor = 0
		LSnome_vd1	 = "SEM VINCULO "
		SHOW GET LNvendedor
		SHOW GET LSnome_vd1
		SELECT clientes
		RETURN .T.
	ENDIF
	IF LNvendedor = 9999
		LSnome_vd1	 = "TODOS VINCULADOS "
		SHOW GET LNvendedor
		SHOW GET LSnome_vd1
		SELECT clientes
		RETURN .T.
	ENDIF
	IF !SEEK(LNvendedor)
		SELECT clientes
		RETURN .F.
	ENDIF
ENDIF

LNvendedor	 = usuario.usuario
LSnome_vd1	 = usuario.nome
SELECT clientes
SHOW GET LNvendedor
SHOW GET LSnome_vd1

RETURN .T.


