	ENDIF
	IF LFcnx701  		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("clianx_a")
	ENDIF
	IF LFcta701  		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("contait")
	ENDIF
	IF LFnfs701  		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("nota")
	ENDIF
	IF LFemp701  		&& .t. =>  DEVE SER FECHADA NA SAIDA
