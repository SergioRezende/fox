FUNCTION ULVldDoc
	=W_DEFPROC("CLIENTES.SPR")
	LNtmp = CLCalc_cgc(m.cgc)
	IF  isadding AND LNtmp = "3"
		RETURN .F.
	ENDIF

	IF LNtmp = "1" 		&& CPF
