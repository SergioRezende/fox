PROCEDURE BTN_VAL1
	PARAMETER tecla, m.chv_ler, m.chv_compara, m.chv_brow
	SELECT fornece
	SET ORDER TO TAG nome
	=W_DEFPROC("CLIENTES.SPR")
	IF isediting AND tecla = 'EDIT' && COMANDO PARA GRAVAR
		IF !CLInscricao((m.ESTADO),(m.INSCRICAO),1,0,0,'')
			WAIT WINDOWS "Nro. Inscricao Invalido..<ENTER>"
			_CUROBJ = OBJNUM(m.inscricao)
			RETURN(.F.)
		ENDIF
	ENDIF

	=ULVldDoc()
	SELECT fornece

    DO btn_val with tecla, m.chv_ler, m.chv_compara, m.chv_brow, .T., .T.

	if isediting
		SHOW GET m.testa_tp disable
	else
		SHOW GET m.testa_tp enable
	ENDIF

	SET ORDER TO TAG codigo
    IF tecla $ 'ADD/EDIT'
		m.dt_alter = wp_dtoper
		SHOW GET m.dt_alter
       	RETURN
    ENDIF
	=W_DEFPROC("MUNICPIO.SPR")
	m.muni_ibge = MNGet_MUNI_IBGE(m.Estado,m.cidade)

	SHOW GET m.muni_ibge

	SELE tab001
	SEEK 'INS'+STR(m.tp_inscr,1)
	m.nome_inscr = tab001.descricao
	SHOW GET m.tp_inscr
	SHOW GET m.nome_inscr
	SELECT fornece
	m.strcgc = str(m.cgc,14)
	SHOW GET m.strcgc

RETURN



