FUNCTION ULvld_UF
PARAMETERS  PrmUF

PRIVATE LSalias,LFretorno


	LFretorno = .t.
	
	LSalias =ALIAS()
	
	
	DO CASE
	
		CASE LASTKEY() = 27 OR LASTKEY() = 15
		   LFretorno = .T.

		CASE LASTKEY() = 9
			=W_DEFPROC("ESTADOS.SPR")
			PrmUF = UFView(PrmUF)
		    IF LASTKEY() = 27
			   LFretorno = .F.
		   ENDIF
	
		OTHERWISE
			=W_DEFPROC("ESTADOS.SPR")
			IF !UFVld_Estado(PrmUF)
				LFretorno =  .F.
			ENDIF
	ENDCASE

	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF
RETURN(LFretorno)


