FUNCTION ULvalParc
	STORE 0 TO 	LGtd1,LGtd2,LGtd3,LGtd4,LGtd5
	DO CASE
		CASE m.vlr_ent > 0
			LGtd1 = 0
			DO CASE
				CASE m.forma_pgto = 5
					LGtd2 = 1
				CASE LNnumparc = 1
					LGtd2 = 31
				CASE LNnumparc = 2
					LGtd2 = 31
					LGtd3 = 61
				CASE LNnumparc = 3
					LGtd2 = 31
					LGtd3 = 61
					LGtd4 = 91
				OTHERWISE
					RETURN(.F.)
			ENDCASE
		CASE m.vlr_ent = 0	
			DO CASE
				CASE m.forma_pgto = 5
					LGtd1 = 1
				CASE LNnumparc = 1
					LGtd1 = 31
				CASE LNnumparc = 2
					LGtd1 = 31
					LGtd2 = 61
				CASE LNnumparc = 3
					LGtd1 = 31
					LGtd2 = 61
					LGtd3 = 91
				OTHERWISE
					RETURN(.F.)
			ENDCASE
	ENDCASE

	m.prazo = STRTRAN(STR(LGtd1,3)," ","0")+SUBS(m.prazo,4)
	m.prazo = LEFT(m.prazo,4)+STRTRAN(STR(LGtd2,3)," ","0")+SUBS(m.prazo,8)
	m.prazo = LEFT(m.prazo,8)+STRTRAN(STR(LGtd3,3)," ","0")+SUBS(m.prazo,12)
