		    	m.dtPrimeira,;
			    m.diadovenc,;
			    m.FixaDiaSem,;
			    m.AjstFimSem,;
		    	m.Tp_Parcela)

	SHOW GET m.prazo


RETURN



FUNCTION ULdefMoeda
	PARAMETERS LNbnc, LNmoeda
	
	*-----------------------------------------------------------------*
	* 	 Cada Cobrador (LNbnc) atua em determinado seguimento com um
	* tipo especifico de moeda. Com base nesta rela‡Æo esta rotina
	* informa a moeda conforme o LNbnc informado.
	*-----------------------------------------------------------------*
	PRIVATE LSbnc
	LSbnc = STR(LNbnc,3)
	LSbnc = CHRTRAN(LSbnc, " ","0")
	
	DO CASE
		CASE LSbnc $ "993" 			&& CAIXA
			LNmoeda = 1				&& Dinheiro (PadrÆo)
		CASE LSbnc $ "854/855/856/857/858/865/866/" 	&& Cartao de  Debito
			LNmoeda = 4	
		CASE LSbnc $ "898" 			&& Cheque em Garantia
			LNmoeda = 2	
		CASE LSbnc $ "850/851/852/853/859/861/862/863/864/"
		 	&& CartÆo de Cr‚dito /BNDES
			LNmoeda = 5	
			
		CASE LSbnc $ "859" 	&& CartÆo de Cr‚dito BNDES
			LNmoeda = 5	
	
		CASE LSbnc $ "860" 	&& CartÆo de Cr‚dito TICKET CAR
			LNmoeda = 5	
		
		CASE LSbnc $ "867" 	&& BRASIL CARD
			LNmoeda = 5	

		CASE LSbnc $ "001/000/237/" 	&& Carteira /Bancos
			LNmoeda = 3
		OTHERWISE
			LNmoeda = 99			&& NÆo Definida
	ENDCASE


RETURN(.T.)
	

