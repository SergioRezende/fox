	IF m.tp_parcela <> 4
		RETURN(.T.)
	ENDIF

	IF m.vlr_ent > 0
		m.ComEntrada = "S"
	else
		m.ComEntrada = "N"
	ENDIF
	
	
	=W_DEFPROC("SVD0204A.SPR")
