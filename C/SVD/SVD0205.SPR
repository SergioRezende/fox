		_CUROBJ  = OBJNUM(m.vlrprc2)
		RETURN(.T.)
	ENDIF

	IF m.vlrprc2 > 0 AND (m.vlrprc2 > m.valor - m.vlr_ent)
		return(.f.)
	ENDIF
	IF m.vlrprc2 =  0
