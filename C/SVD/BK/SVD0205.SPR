FUNCTION  ULcalcVlres

	m.vlr_ent = m.vlrent1 + m.vlrent2

	m.vlrent1 = m.vlr_ent - m.vlrent2
	m.vlrent2 = m.vlr_ent - m.vlrent1
	m.vlrprc1 = m.valor - m.vlr_ent - m.vlrprc2
	m.vlrprc2 = m.valor - m.vlr_ent - m.vlrprc1

	SHOW GET m.vlrent1
	SHOW GET m.vlrent2
	SHOW GET m.vlrprc1
	SHOW GET m.vlrprc2

	SHOW GET m.moedaen1
	SHOW GET m.bnc_ent1
	SHOW GET m.moedaen2
	SHOW GET m.bnc_ent2
	
	SHOW GET m.moedapr1
	SHOW GET m.bnc_prc1
	SHOW GET m.moedapr2
	SHOW GET m.bnc_prc2

RETURN


