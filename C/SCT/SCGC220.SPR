	LSinsc	= 	CHRTRAN(LSinsc,".","")
	LSinsc	= 	CHRTRAN(LSinsc,"/","")
	LSinsc	= 	CHRTRAN(LSinsc,"-","")
	LSinsc	= 	LEFT(LSinsc+space(14),14)


	LS_tp50	=	"50"
