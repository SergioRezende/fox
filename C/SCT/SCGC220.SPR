		LSvlr_icms	=   CHRTRAN(LSvlr_icms,",","")						
		LSvlr_icms	=   CHRTRAN(LSvlr_icms,".","")						

		LSbase_isent=	str(LIVROENT.base_isent,14,2)						
		LSbase_isent=   CHRTRAN(LSbase_isent," ","0")						
		LSbase_isent=   CHRTRAN(LSbase_isent,",","")						
		LSbase_isent=   CHRTRAN(LSbase_isent,".","")						
		
