	*-------------------------------------------------------------------*

	SELECT LIVROENT
	SET ORDER TO TAG rldtentr
	SET NEAR ON
	SEEK STR(LNemp,3)+DTOS(LDper_ini)
	SET NEAR OFF

	DO WHILE !EOF()
		LSmsg = "Produtos Entrada (54) "+;
				 	STR(LIVROENT.empresa,3)+"-"+DTOC(LIVROENT.DATA)+;
					STR(LIVROENT.NOTA,6)
