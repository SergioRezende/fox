PROCEDURE ULfecha
	IF LFapu222  		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("apuraicm")
	ENDIF
	IF LFemp222  		&& .t. =>  DEVE SER FECHADA NA SAIDA
