			ENDIF
			m.valor = LNsoma_07


			LNnroNFAux = subs(str(nota.nota,7),2)
			IF FOUND()
				m.descricao = "NF."+;
							TRANSF(LNnroNFAux,"@r 999,999")+" "+ ;
							ALLTRIM(m.descricao)+"//TRF.P/"+ ;
							empresa.sigla
			ELSE
				m.descricao = "NF. "+;
							TRANSF(LNnroNFAux,"@r 999,999")+" "+ ;
							itemmov.codigo+"-"+;
							LEFT(itemmov.CLASSIFICA,5)
			ENDIF
			SELE &wl_arqtmp
			=edithand('SAVE')
			SET NEAR OFF
				***
			LSqbrcodigo = itemmov.codigo
			LSquebra    = itemmov.classifica
			LNsoma_07   = 0
RETURN



