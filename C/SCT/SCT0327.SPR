PROCEDURE ULqbrtrfsub		&& PROCESSA QUEBRA ACESSORIOS
			
			m.descricao = ""
			** GRUPOS DE PRODUTOS TRANSFERIDOS ** ACESSORIOS
			SELE cadcontb
			SET NEAR OFF
			LNtmp = INT(VAL(SUBS(LSquebra,1,2)))
			LNtmp1 = INT(VAL(SUBS(LSquebra,3,3)))
			SEEK "2"+" 4"+STR(LNtmp,2) +STR(LNtmp1,3)
