        N.codforn,N.inscricao,;
        N.UFOrig,N.MunOrig,;
        N.UFDstn,N.MunDstn,;
        N.uf,;
        N.tipo,N.data,N.data_emi,;
		N.vlrfrete,N.vlrseguro,N.vlrdespes,;
        N.status,;
        (I.vlrvenda+I.vlripi+I.icms_subs) AS VlrNota,;
		N.aliq_icms,;
		ULdefCfo(N.empresa,N.tipo,i.tp_mercad,"ENTRADA") AS SET_CFO;
   	FROM  &dbf_NFETMP N,;
   	      &dbf_TIPTMP T,;
   	      &dbf_INETMP I ;
    WHERE ;
            LEFT(N.situacao,1) = "C";
		AND N.ch_produ <> "9";
