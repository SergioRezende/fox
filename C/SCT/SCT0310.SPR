    GROUP BY N.EMPRESA,N.DATA,N.NOTA,SET_CFO,I.ALIQ_ICMS;
    INTO CURSOR TMPLIVR


	SELECT 	;
		L.*,;
		 ULCST_Entrada(I.cst, I.origem, I.tp_mercad,I.base_calc,;
