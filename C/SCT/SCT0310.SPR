FUNCTION IV_TMPMOV
PARAMETERS PrmEmp,PrmDtFim

	*-------------------------------------------------------------------*
	*-------------------------------------------------------------------*
	SELECT 	;
           IT.EMPRESA,;
           IT.CODIGO,;
           IT.QTDE,;
           IT.TIPO,;
           IT.OPERACAO,;
           IT.DATA,;
           IT.MOVESTQ;
    FROM GRUPO GR,;
         ITEMMOV IT;
    WHERE ;
        	gr.cdg_tipo = 4;
        AND (gr.tp_control = 1 or gr.tp_control = 4);
        AND STRTRAN(STR(PrmEmp,3),' ','0') $ gr.empresas ;
        AND IT.EMPRESA = PrmEmp ;
     	AND IT.CODIGO = GR.CODIGO ;
        AND YEAR(IT.DATA) = YEAR(PrmDtFim);
    ORDER BY GR.CODIGO;
   	INTO TABLE  \TMP\TMP_MOV

*---------------------------------------------------------------------*
	GO TOP

	
RETURN



