   CHR(13)+CHR(10)+'VLCANICMS="'+VLCANICMS+'"'+;
   CHR(13)+CHR(10)+'VLCAN="'+VL_CAN+'"' AS PARTE_3;
  FROM X_ITEM0 INTO CURSOR X_ITEM1



  SELECT ;
   (COD_MOD+DT_DOC+ECF_CX+ALIQUOTA+CFOP) AS CHAVE,;
   CHR(13)+CHR(10)+'VLDSCISSQN="'+VLDSCISSQN+'"'+;
