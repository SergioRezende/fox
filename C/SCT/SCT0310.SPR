			DATA, ;
			ALIQ_ICMS, ;
			"XX"			 AS UF, ;
		 	0    AS TOTAL_NOTA,;
		 	0	 AS BASE_ICMS,;
			0	 AS BASE_ISENT,;
			0	 AS BASE_OUTR,;
			0	 AS BASE_ISS,;
			0	 AS BASE_SUBS,;
		 	SUM(TOTAL_NOTA)  AS CANCELA,;
			SUM(TOTAL_NOTA)  AS VNDABRUTA;
	FROM &dbf_NFSTMP ;
		WHERE   status = 2 ;
		    AND  operacao  = "S" ;
		    AND  cupom > 0 ;
