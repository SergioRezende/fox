#REGION 1
PROCEDURE ULfecha
	IF LFmapa   		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("mapaecf")
	ENDIF
	IF LFemp   		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("empresa")
	ENDIF
	wp_flgfecha = .F. 		&& defaut nao fechamento da secao
