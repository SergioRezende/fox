#REGION 1
PROCEDURE ULfecha
	IF LFemp102		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("empresa")
	ENDIF
	IF LFfun102		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("funcao")
	ENDIF
	IF LFusr102		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("usuario")
	ENDIF
	wp_flgfecha = .F. 		&& defaut nao fechamento da secao
	IF !EMPTY(LS102areant)
		SELECT &LS102areant
	ENDIF
	ON KEY LABEL ESCAPE
	SET FORMAT TO
RETURN

*************
*-------------------->>>>>>>>>
PROCEDURE BTN_VAL1
	PARAMETER tecla, m.chv_ler, m.chv_compara, m.chv_brow

	SELE usuario
	ON KEY LABEL ENTER
    DO btn_val with tecla, m.chv_ler, m.chv_compara, m.chv_brow
    IF tecla $ 'ADD/COPY'
		m.senha = ""
    ENDIF
    IF tecla $ 'ADD/EDIT'
       RETURN
    ENDIF
	sele empresa
	SET ORDER TO TAG empresa
	SEEK m.emp_ini
	m.nome_emp   = empresa.nome
	SHOW GET m.nome_emp

	SELE funcao
	SET ORDER TO TAG nivel

	SEEK m.nivel_com
   	m.nivel_com = funcao.nivel
   	m.nmfun_com  = funcao.descricao
   	SHOW GET m.nivel_com
   	SHOW GET m.nmfun_com

	SEEK m.nivel_aces
   	m.nivel_aces = funcao.nivel
   	m.nmfun_aces = funcao.descricao
   	SHOW GET m.nivel_aces
   	SHOW GET m.nmfun_aces
    SELECT usuario

RETURN

PROCEDURE ULerro
	LNerro = ERROR()
RETURN

