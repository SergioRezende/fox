#REGION 1
PROCEDURE ULfecha
	IF LFfmt107		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("formato")
	ENDIF
	IF LFind107		&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("indice")
	ENDIF
	wp_flgfecha = .F. 		&& defaut nao fechamento da secao
