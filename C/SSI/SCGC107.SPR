*
FUNCTION _2lx0w1863     &&  m.dbf VALID
#REGION 1
ON KEY LABEL ESCAPE KEYBOARD "{BACKTAB}"

LSarq = m.dbf
LFdbffecha	= .F.
IF !USED(ALLTRIM(LSarq))
	LFdbffecha	= .T.
	IF ! NetUse(LSarq)
		wp_flgfecha = .t.  && implica no fechamento da secao
	ENDIF
