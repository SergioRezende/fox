PROCEDURE BTN_VAL1
	PARAMETER tecla, m.chv_ler, m.chv_compara, m.chv_brow
	SELE indice
    DO btn_val with tecla, m.chv_ler, m.chv_compara, m.chv_brow
    IF tecla $ 'ADD/EDIT'
       RETURN
    ENDIF
	STORE "---" TO wv_indice
	SHOW GET LNind
	SELE formato
	SEEK m.dbf+m.variacao
	m.mascara  = formato.mascara
	SHOW GET m.mascara
	SELECT indice

RETURN

