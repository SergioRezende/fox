PROCEDURE BTN_VAL1
	PARAMETER tecla, m.chv_ler, m.chv_compara, m.chv_brow
	SELE acessos
    DO btn_val with tecla, m.chv_ler, m.chv_compara, m.chv_brow
    IF tecla $ 'ADD/EDIT'
		SHOW GET m.programa ENABLE
       RETURN
    ENDIF
	SHOW GET m.programa DISABLE
	SELE funcao
	SEEK m.usuario
	m.nome = funcao.descricao
  	SELECT ACESSOS
    SHOW GET m.nome
RETURN



