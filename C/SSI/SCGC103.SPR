*
FUNCTION _2lx0w0p32     &&  m.usuario VALID
#REGION 1
ON KEY LABEL ESCAPE KEYBOARD "{BACKTAB}"
select funcao
if lastkey() = 9
   ON KEY LABEL ESCAPE
   DO btn_val WITH 'LOCATE'
   ON KEY LABEL ESCAPE KEYBOARD "{BACKTAB}"
   m.usuario = funcao.nivel
   m.nome    = funcao.descricao

   select acessos
   IF LASTKEY() = 27
		RETURN .F.
   ENDIF
   show get m.usuario
   SHOW GET m.nome
   return .T.
endif
if seek(m.usuario)
   m.usuario = funcao.nivel
   m.nome    = funcao.descricao
   select acessos
   show get m.usuario
   SHOW GET m.nome
   return .t.
endif
select acessos
return .f.


