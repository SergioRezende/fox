SELECT CL.CLIENTE,CL.NOME,;
        IT.NOTA,IT.DATA,;
        IT.CODIGO,;
        IT.DESCRICAO,;
        IT.QTDE,;
        IT.VLRVENDA,;
        IT.VLRIPI ;
FROM X:\SCGC\LOJA\CLIENTES CL,;
     X:\SCGC\LOJA\ITEMMOV IT;
WHERE   IT.CH_OPERA = "1" ;
    AND LEFT(IT.OPERACAO,1) = "S" ;
    AND CL.CLIENTE = IT.FAVORECIDO ;
    AND IT.DATA >= {01.09.2006} ;
    AND IT.DATA <= {30.09.2006} ;
    AND LEFT(IT.SITUACAO,1)="O";
ORDER BY CL.NOME,IT.CODIGO,IT.DATA ;
INTO TABLE C:\TMP\VENDAS.DBF
    

     