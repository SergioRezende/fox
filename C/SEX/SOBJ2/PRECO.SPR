*---------------------------------------------------------------*
FUNCTION ANTPRpreco
PARAMETERS LNemp,LSnatu_oper,LSTpOper,LScodigo,LDdata,LNpreco,LSclascms
	*------------------------------------------------------------*
	* CLASSIFICACAO:  [*****]
	*------------------------------------------------------------*
	* OBJETIVO....: Informar Pre뇇 de Tabela do Produto
	*------------------------------------------------------------*
	* COMENTARIO..:   A funcao permite identificar :
	*
	*					- TABELA EM VIGOR : Conforme a data passada
	*				como parametro;
	*
	*					- EMPRESA DE REFERENCIA : Duas Empresas podem
	*				usar uma tabela comum para evitar redundancia de
	*				informa뉏es ;
	*					EX : SIA usa tabela 2 (EMPTAB = 2)
	*						 W-3 refere a mesma (EMPTAB = 2)
	*
	*					- PRECO DIFERENCIADO PARA :
	*						Transferencia
	*						Vendas E Outras
	*------------------------------------------------------------*
	* OBS........ :
	*------------------------------------------------------------*
	*  TABELAS....:
	*------------------------------------------------------------*
	*  PARAMETROS..:
	*		LNemp..........: Empresa que se quer obter informacoes
	*		LSnatu_oper....: Indicativo da Natureza da Opercao
	*							(2) = 	Transferencia
	*							(<>2)	=	Outras Operacoes
	*						  Alteram a forma de definir o preco
	*		LSTpOper.......: TIPO Identificador da Operacao Comercial
	*		LScodigo..........: Codigo do produto
    *       LDdata.........: Data para Refereciar a tabela em Vigor
	*
	*------------------------------------------------------------*
	*  RETORNO.....:
	*		LNpreco........: Preco Localizado
	*		LSclascms......: Grupo de Comissao 		
	*------------------------------------------------------------*
	* EXEMPLO : CHAMADA DO PROGRAMA ?
	*------------------------------------------------------------*
	=W_DEFPROC("rotinas.spr")
	PRIVATE LSalias
	PRIVATE LFempresa,LFsaldo,LFpreco,LFgrupo,LFtipooper
	PRIVATE LNtbVigor, LSsrVigor
	PRIVATE LNtp_mercad
	*-----------------------------------------------------------*
	IF LEFT(LScodigo,5) = "99999"	&& GENERICOS
		RETURN(.T.)
	ENDIF
	*-----------------------------------------------------------*
	LSalias	= ALIAS()
	*-----------------------------------------------------------*
	STORE 0  TO LNtbVigor,LNtp_mercad
	STORE "" TO LSsrVigor

	*STORE 0  TO LNpreco,LNtbVigor,LNtp_mercad
	*STORE "" TO LSclascms, LSsrVigor

	IF TYPE("LSnatu_oper") = "N"
		LSnatu_oper = STR(LSnatu_oper,1)
		LSnatu_oper	=	CHRTRAN(LSnatu_oper," ","0")
	ENDIF

	LFempresa		= NetArq("empresa")
	LFsaldo			= NetArq("saldo")
	LFpreco			= NetArq("preco")
	LFgrupo			= NetArq("grupo")
	LFtipooper		= NetArq("tipooper")
	IF (LFempresa+LFsaldo+LFpreco+LFgrupo+LFtipooper) > 100000
		DO PRfcpreco
		RETURN(.f.)
	ENDIF
	LFretorno 	= .T.

	IF !PRtabvigor((LDdata),LNtbVigor, LSsrVigor)
		DO PRfcpreco
		RETURN(.f.)
	ENDIF

	SELE empresa
	SET ORDER TO TAG empresa
	SEEK LNemp
	IF !FOUND()
		DO PRfcpreco
		RETURN(.f.)
	ENDIF
	
	SELE GRUPO
	SET ORDER TO TAG codigo
	SEEK LScodigo
	IF !FOUND()
		DO PRfcpreco
		RETURN(.f.)
	ENDIF

	SELECT tipooper
	SET ORDER TO TAG tipo
	SEEK 'S'+LSTpOper
	IF !FOUND()
		SEEK 's'+LSTpOper
	ENDIF
	IF !FOUND()
		DO PRfcpreco
		RETURN(.f.)
	ENDIF

	*------------------------------------------------------------*
    IF LSnatu_oper = "2" && TRANSFERENCIAS
		=W_DEFPROC("ORCAMENT.SPR")
	   	IF !ORDef_TpMercad((LNemp),(LScodigo),(LSTpOper),;
						(LDdata),LNtp_mercad)
			DO PRfcpreco
			RETURN(.f.)
		ENDIF
		*------------------------------------------------------------*
		SELECT saldo
	   	SET ORDER TO TAG clas_saldo
	   	SEEK STR(LNemp,3)+GRUPO.classifica+;
    	    STR(YEAR(LDdata),4)+STR(MONTH(LDdata),2)
	   	IF FOUND()
			IF saldo.sld_atu = 0
		        LNpreco		=	saldo.vlr_atu / 1
			else
		        LNpreco		=	saldo.vlr_atu / saldo.sld_atu
			ENDIF
			***************************************************************
			*	      TRANSFERENCIA INTERNA DE MERCADORIA COM RETENCAO    *
			*   ENTRA NO CALCULO DE INDICES ESPECIFICOS					  *
			***************************************************************
			IF tipooper.ch_desti = "1"
				DO CASE
					CASE	LNtp_mercad = 1
				        LNpreco	= LNpreco/((100 - tipooper.aliq_icms) / 100)
					CASE	LNtp_mercad = 2
						************************************************
						*    Inicio de tratamento nao parametrizavel
						*	( ESPECIFICO)
						*	Para PNEUS Ind,Caminhao,Tratores,Agric,	
						*	Terrapl > "00448"  e < "00800"	
						************************************************
						IF LEFT(grupo.classifica,5) > "00448" AND ;
						   LEFT(grupo.classifica,5) < "00800"
						        LNpreco = LNpreco * 1.24
						ELSE
						        LNpreco = LNpreco * 1.26
						ENDIF				
					OTHERWISE
						LNpreco = LNpreco
				ENDCASE
			ELSE
		        LNpreco = LNpreco / ((100 -  tipooper.aliq_icms) / 100)
			ENDIF
			**************************************************************
			*    FIM de tratamento nao parametrizavel ( ESPECIFICO)
			**************************************************************
		    SELECT preco
    		SET ORDER TO TAG tabela
	    	IF SEEK(STR(empresa.emptab,3)+STR(LNtbVigor,3)+;
				LSsrVigor+LScodigo)
		       		LSclascms = preco.clas_cms
			ENDIF
	   ENDIF
	ELSE
	    SELECT preco
    	SET ORDER TO TAG tabela
	    IF SEEK(STR(empresa.emptab,3)+STR(LNtbVigor,3)+LSsrVigor+LScodigo)
	       LNpreco 		=   preco.preco
	       LSclascms 	=	preco.clas_cms
	    ELSE
			LFretorno 	= .F.
	        LNpreco 	=	0
	        LSclascms 	=	""
	    ENDIF
	ENDIF
	*------------------------------------------------------------*
	DO PRfcpreco
RETURN(LFretorno)

PROCEDURE PRfcpreco
		=UP_fecha("empresa" ,LFempresa)
		=UP_fecha("saldo" ,LFsaldo)
		=UP_fecha("preco" ,LFpreco)
		=UP_fecha("grupo" ,LFgrupo)
		=UP_fecha("tipooper" ,LFtipooper)
		IF !EMPTY(LSalias) AND USED(LSalias)
			SELECT &LSalias
		ENDIF
RETURN


