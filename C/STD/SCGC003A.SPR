			       chr(13)+" Elimine o Orcamento Para Prosseguir."+ ;
			       chr(13)+" Doc : "+m.tipo+"  "+str(m.orcamento,6)
				   wp_flgfecha = .t.   				
			ENDCASE
			SELE notaimpo
			SKIP
			=UPtransacao("ABORTAR")
			LOOP
		ELSE
			SELE itemmov
			SET ORDER TO TAG orcamento
			SEEK STR(m.empresa,3)+STR(m.orcamento,6)
