FUNCTION ULCvNfCanCpm

		PRIVATE LFnota
				
		PRIVATE LAnota

		PRIVATE LDnota


		LDnota = m.drivetmp+m.nomearq+"NF"


		LFnota  = NetArqAgain("nota")
		LAnota  = ALIAS()


		IF (LFnota) > 100000
			=UP_fecha("&LAnota")
			wp_msg = "Arquivo de N.F. nao pode ser aberto."
			RETURN .f.
		ENDIF

		SELE &LAnota
		SET ORDER TO TAG data
		SET NEAR ON
		SEEK STR(wp_empresa,3)+DTOS(m.dt_inicio)
		IF m.saida = 1
			COPY TO &LDnota ;
			      FOR &LAnota .status = 2 ;
			      OR  &LAnota .tipo = "CPM"  ;
			      OR  &LAnota .tipo = "ENT"  ;
			      WHILE;
				       ( &LAnota .data >= m.dt_inicio ;
				     AND &LAnota .data <= m.dt_fim) ;
 				TYPE SDF
		ELSE
			COPY STRU TO &LDnota
			COPY TO &LDnota  ;
			      FOR &LAnota .status = 2 ;
			      OR  &LAnota .tipo = "CPM"  ;
			      OR  &LAnota .tipo = "ENT"  ;
			     WHILE;
				      ( &LAnota .data >= m.dt_inicio ;
				    AND &LAnota .data <= m.dt_fim)
		ENDIF							
		=UP_fecha("&LAnota")

RETURN(.t.)


