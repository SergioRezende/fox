PROCEDURE ULabreCaixa
	PARAMETERS LNempresa,LDdata
	SELE cxdiario
	SET ORDER TO TAG prk_cxdiar
	SET NEAR ON
	SEEK STR(LNempresa,6)+DTOS(LDdata)
	SET NEAR OFF
	IF FOUND()
		WAIT window "Caixa ja Esta Aberto. "
		RETURN(.F.)
	ENDIF

	SKIP -1
	IF BOF() OR LNempresa <> cxdiario.id_empresa
		WAIT window "Nao Existe Caixa Anterior. "
		RETURN(.F.)
	ENDIF
	SCATTER	MEMVAR
	m.id_data		=	LDdata
	m.indinheiro	=	m.fndinheiro
	m.inchqpre		=	m.fnchqpre
	m.fndinheiro	=	0
	m.fnchqpre		=	0
	=EDITHAND("SAVE")

	=BTN_VAL1('ATUALIZA', VLleitura, Vlcompara)
RETURN

			
