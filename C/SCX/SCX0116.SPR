		M.TADevReg		= M.TADevReg  - ARQ_TMP1.devolucao
		=EDITHAND("APAGA")
	ENDIF
	
	*  GRAVANDO ARQ DEFINITIVO *
	
	SELE docRCBcX
	SET ORDER TO TAG PRK_CHAVE
	SEEK STR(m.ID_EMPRESA,6)+STR(m.ID_LANCAME,6)+STR(m.ID_DUPLICA,9)
