	SELE ARQ_TMP1
	SET ORDER TO TAG PRK_CHAVE
	SEEK STR(m.ID_EMPRESA,6)+STR(m.ID_LANCAME,6)+STR(m.ID_DUPLICA,9)
	IF FOUND()
		M.ANT_RECEBIDO	= ARQ_TMP1.sld_liquid + ARQ_TMP1.despesas +;
					  ARQ_TMP1.juros - ARQ_TMP1.desconto +;
