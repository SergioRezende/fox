PROCEDURE BTN_VAL1
	PARAMETER tecla, m.chv_ler, m.chv_compara, m.chv_brow
	SELE promocao
	SET ORDER TO TAG promocao
	IF tecla = "PRINT"
        LDdtinicio  = DATE()
        LDdtfim     = DATE()
        LFmtvretorno="NORMAL"
        LFtpdtfim   = .T.
        DO obj_dat1.spr WITH LDdtinicio,LDdtfim,LFmtvretorno,LFtpdtfim
        LSNemp   = wp_empresa
        LDInicio = LDdtinicio
        LDFim    = LDdtfim
        = RELPROM(LSNemp,LDInicio,LDFim)
	   ELSE
	    DO btn_val with tecla, m.chv_ler, m.chv_compara, m.chv_brow,.T.,.F.
	ENDI
    m.empresa  = wp_empresa
	if tecla $ 'ADD/EDIT'
       return
    endif
    SELE GRUPO
    SET ORDER TO TAG codigo
    SEEK m.codigo

	SELECT promocao
	SHOW GET 	grupo.descricao
	SHOW GET 	grupo.ordem
RETURN


PROCEDURE local_refresh
PARAMETERS wl_branco
	IF isediting  or wl_branco
		SHOW GET com_btn DISABLE
	ENDIF
RETURN


PROCEDURE RELPROM
    PARAMETERS LSNemp,LDInicio,LDFim

	SELE PROMOCAO
	SET ORDE TO PERIODO

	SELE GRUPO
	SET ORDER TO TAG codigo

    SELE 40
    SELECT PROMOCAO.EMPRESA,PROMOCAO.DT_INICIO,PROMOCAO.DT_FIM,;
			GRUPO.CODIGO,GRUPO.DESCRICAO,PROMOCAO.PRECO,;
			PROMOCAO.DESC_VAREJ,PROMOCAO.DESC_REVEN,PROMOCAO.DESC_PPUBL,;
			PROMOCAO.DESC_FROTA;
   	FROM PROMOCAO,GRUPO ;
	ORDER BY GRUPO.ORDEM ;
	WHERE !EMPT(GRUPO.CODIGO) AND !EMPT(GRUPO.DESCRICAO);			
			AND PROMOCAO.CODIGO=GRUPO.CODIGO AND;
		    PROMOCAO.PRECO > 0 AND;
			!(promocao.dt_fim < LDINICIO or ;
			promocao.dt_inicio > LDFIM);
    INTO CURSOR C_PROMOCAO

IF EMPT(C_PROMOCAO.EMPRESA)
    WAIT WIND "Nao ha dados neste periodo !"
    RETU
ENDI

