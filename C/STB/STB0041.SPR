	*-----------------------------------------------------------------*
	
	m.subgrupo = ALLTRIM(m.subgrupo)
	SELECT grfiscal
	SET ORDER TO tributacao
	SET NEAR ON
	
	SEEK m.uf+m.subgrupo

RETURN(UPtratachv())


