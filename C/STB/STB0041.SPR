FUNCTION _2lx0wa63c     &&  m.subgrupo VALID
#REGION 1
	=W_DEFPROC("grupo.spr")

	LNkey 	= LASTKEY()
	LScond_Brows = ".T."

	IF LNkey = 9
		IF !GRLoc_Prod(m.subgrupo,LNkey, LScond_Brows )
		    RETURN .F.
		ENDIF
	ENDIF
	*-----------------------------------------------------------------*
