FUNCTION AULlecodigo  && RETORNA O NOME DO CODIGO INFORMADO GRUPO//SUBGRUPO
PARAMETERS LScdg
	LSalias = ALIAS()

	SELE GRUPO
	LNrgtmp = RECNO()
	SET ORDER TO classifica
	SEEK LScdg
	LSnome = grupo.descricao
	SET ORDER TO ordem
	GO LNrgtmp
	
	SELE &LSalias
RETURN(LSnome)


FUNCTION ULlecodigo  && RETORNA O NOME DO CODIGO INFORMADO GRUPO//SUBGRUPO
PARAMETERS LScdg
	LSalias = ALIAS()

	SELE GRUPO
	LNrgtmp = RECNO()
	SET ORDER TO classifica

	SEEK LScdg
	LSnome = ALLTRIM(grupo.descricao)


	SET ORDER TO ordem
	GO LNrgtmp

	LFQBR = .T.
	
	SELE &LSalias
RETURN(LSnome)


