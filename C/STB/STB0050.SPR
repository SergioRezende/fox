*>>>>>>>>>>>>>>>>>>>>>>  2A ETAPA / 3     <<<<<<<<<<<<<<<<<<<<<<<<
*          GERAR ARQUIVO DBF COM BASE NO TEXTO CONTINUO            *
*------------------------------------------------------------------*

	wl_arqtmp = ""
	wl_arqtb1 = ""		&& ORIENTA A 1A COLUNA
	wl_arqtb2 = ""		&& ORIENTA A 2A COLUNA
