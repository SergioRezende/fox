*       є Object Type:         Push Button                        є
*       є Snippet Number:      13                                 є
*       є                                                         є
*       УДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДЅ
*
FUNCTION _2lx0wbk3y     &&  m.imp_btn VALID
#REGION 1
	SELE grupo
    SET ORDER TO TAG classifica
	SET NEAR ON
    SEEK m.clasinicio
	SET NEAR OFF
	DO CASE
		CASE m.flordem = 1
			SET ORDER TO TAG codigo
		CASE m.flordem = 2
			SET ORDER TO TAG ordem
		CASE m.flordem = 3
			SET ORDER TO TAG tab_comiss
		CASE m.flordem = 4
			SET ORDER TO TAG classifica
	ENDCASE			
*******************
*---->   (INICIALIZACAO DO CONTROLE DE STATUS IMPRESSAO)
*******************	
	DO RELINIC.PRG          && INICIA VAR P/ TERMOMETRO
	LFsegue = .t.
	LNregistro = RECNO()
	LNimpressao = 0
    COUNT  WHILE  grupo.classifica <> m.clasfim TO LNimpressao
	LNimpressos = 0
    GO LNregistro
	
	DO WHILE !EOF() AND grupo.classifica <> m.clasfim AND LFsegue
	    =UPtermo()
		************** INICIO PREPARA AVANCO P/PROXIMO REGISTRO ******
		SKIP
		LNproximo = 0
		IF !EOF()
			LNproximo = RECNO()
			SKIP - 1
		ELSE
			GO BOTT
		ENDIF	
		************** FIM PREPARA AVANCO P/PROXIMO REGISTRO ******
	
		m.ordem   = m.sequencia
		m.sequencia = m.sequencia + m.incr
		=REGLOCK(.T.)
		***************************
		SET FIELDS TO dtregis, hregis, usrregis,deletado
		SET FIELDS TO ordem
		=edithand('REGRAVA')
		CLEAR FIELDS
		SET FIELDS OFF
		***************************
		SELE preco
		SET ORDER TO TAG  tabela
		SEEK STR(m.empresa,3)+STR(m.tabela,3)+m.serie+grupo.codigo
		IF FOUND()
			=REGLOCK(.T.)
			***************************
			SET FIELDS TO dtregis, hregis, usrregis,deletado
			SET FIELDS TO ordem
			=edithand('REGRAVA')
			CLEAR FIELDS
			SET FIELDS OFF
			***************************
		ENDIF
		SELE grupo
		************** INICIO AVANCO P/PROXIMO REGISTRO ******
		IF LNproximo = 0
			EXIT
		ENDIF
		GO 	LNproximo
		************** FIM AVANCO P/PROXIMO REGISTRO ******
	ENDDO

	DO RELFIM.PRG          && FIM VAR P/ TERMOMETRO
    SHOW GETS
	SHOW WINDOW stb0065 TOP
	SHOW GET m.imp_btn DISABLE
RETURN



