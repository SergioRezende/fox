FUNCTION FNExiste


	PRIVATE LFone
	PRIVATE LFreturn

	PRIVATE LSalias
	LSalias		= 	ALIAS()

	=FNVerifyInst()


	SELE &PBFoneAlias

	LSchaveAcesso  =FNGetPropVT("CHAVE DE LEITURA")


	DO CASE
		CASE UPPER(LSchaveAcesso) = "PFONE"
			LFonee = FNgetPropVT("PK_FONE")
			SET ORDER TO TAG PK_FONE
			SEEK LFone
		OTHERWISE
			LFonee = FNgetPropVT("PK_FONE")
			SET ORDER TO TAG PK_FONE
			SEEK LFone
	ENDCASE


	IF FOUND()
	    LFreturn = .T.
	ELSE
	    LFreturn = .f.
	ENDIF
	*--------------------------------------------------------------*
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF

RETURN(LFreturn)




