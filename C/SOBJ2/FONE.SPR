	SELECT    ;
		DATA+HORA+DIRECAO+FONE AS CHV_PK,;		
	    DIRECAO, ;
		STATUS , ;
   		DATA   , ;
    	DATA_DT, ;
    	HORA   , ;
    	TRONCO , ;
    	FONE   , ;
    	RAMAL  , ;
    	NOME   , ;
   		OPERACAO,;
