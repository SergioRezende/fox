FUNCTION FNCreate
	=W_DEFPROC("rotinas.spr")
	PRIVATE LSalias
	PRIVATE LSAlsTmp
    PRIVATE LMaxDim
	LSalias		= 	ALIAS()
	
	IF TYPE("PBFoneAlias") <> "U" ;
	   		AND !EMPTY(PBFoneAlias) ;
	   		AND USED(PBFoneAlias)
			RETURN(.T.)
	ENDIF


	PUBLIC PBFoneAlias

	IF USED("Fone")
	     =NetArqAgain("Fone")
	     PBFoneAlias     = Alias()
	ELSE
	     =NetArqAgain("Fone")
	     LSAlsTmp     = Alias()
	     =NetArqAgain("Fone")
	     PBFoneAlias     = Alias()
		 IF !EMPTY(LSAlsTmp) AND USED(LSAlsTmp)
			SELECT &LSAlsTmp
			USE
		 ENDIF
	ENDIF	

    SELE &PBFoneAlias
	LMaxDim = Fcount()
    PUBLIC DIMENSION VTFone[LMaxDim]&& VETOR PARA CAMPOS DA TABELA
    PUBLIC DIMENSION VDFone[2,3]	&& VETOR PARA CAMPOS DERIVADAS
    PUBLIC DIMENSION VFFone[1]      && VETOR CABECALHO DOS CAMPOS
    PUBLIC DIMENSION VCFone[8,3]	&& VETOR PARA CONFIGURACOES


	=AFIELDS(VFFone)



	*-----------------------------------------------------------*
	=FNReadProperty()
	=FNSetDerivadas()
	=FNSetConfig()
	*--------------------------------------------------------------*
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF
RETURN



