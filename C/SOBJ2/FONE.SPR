FUNCTION FNSQL01TeleAtd


	SET TALK ON
	set procedure to fone.spr
	=FNimporta()

	=W_DEFPROC("rotinas.spr")


    PRIVATE LSalias
    PRIVATE ARQ_Ligacoes,ALS_Ligacoes
	PRIVATE LSmsg
	PRIVATE LDt_Ini
	PRIVATE LDt_Fim
	PRIVATE LF1, LF2

	LDt_Ini = Date() - 180
	LDt_Fim = Date()

	LF1 = .f.
	LF2 = .f.
	
	do obj_dat1.spr with LDt_Ini, LDt_Fim, LF1, LF2

	IF !fox_alert('Confirma a Processo ? ')
	   RETURN
	ENDIF

    LSAlias      = Alias()

    ARQ_Ligacoes    = NetArqAgain("Ligacoes")
    ALS_Ligacoes    = Alias()
	set step on
	*------------------------------------------------------------*
	SELECT "Total Ligacoes" AS TITULO, COUNT(*) ;
		FROM  &ALS_Ligacoes ;
		WHERE DIRECAO = "I" ;
			AND STATUS = "#" ;
			AND RAMAL $ "137/138/139/140" ;
			AND ctod(DATA) >= LDt_Ini ;
			AND ctod(DATA) <= LDt_Fim ;
	UNION ;
	SELECT "Nao Atendidas " AS TITULO, COUNT(*) ;
		FROM  &ALS_Ligacoes ;
		WHERE DIRECAO = "I" ;
			AND STATUS = "#" ;
			AND OPERACAO = "RNG" ;
			AND RAMAL $ "137/138/139/140" ;
			AND ctod(DATA) >= LDt_Ini ;
			AND ctod(DATA) <= LDt_Fim ;
	INTO CURSOR TMP1
	
	*----------------------------------------------*			
	SELECT * FROM TMP1 ;
	UNION;
	SELECT "Atd.Rm:"+RAMAL AS TITULO, COUNT(*) ;
		FROM  &ALS_Ligacoes ;
		WHERE DIRECAO = "I" ;
			AND STATUS = "#" ;
			AND OPERACAO = "CON";
			AND RAMAL $ "137/138/139/140" ;
			AND ctod(DATA) >= LDt_Ini ;
			AND ctod(DATA) <= LDt_Fim ;
		GROUP BY RAMAL ;
	INTO CURSOR TMP3

	SELECT * FROM TMP3 INTO TABLE Q:\SCGC\CONSULTA\ATD_TELE
	*------------------------------------------------------------*
	SET TALK OFF
    =up_fecha("&ALS_Ligacoes")
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF
RETURN(.t.)



