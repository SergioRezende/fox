FUNCTION  OUIndexaDBF
	PARAMETERS PrmDir,PrmDBF

    *-------------------------------------------------------------*
	LSarqDest  = PrmDir+PrmDBF

	IF ! NetUse( LSarqDest ,.T.)   && ABRIR EM MODO EXCLUSIVO
