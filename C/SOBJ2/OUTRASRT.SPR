		SELE &LSalias
		RETURN(.f.)
	ENDIF
	LSaliasDestino = ALIAS()

	SET TALK ON

	SELE &LSaliasDestino
	PACK
	REINDEX

	SET TALK OFF

	=UP_fecha(PrmDBF)

