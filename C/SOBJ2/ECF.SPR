			DO OBJ_MENS.SPR WITH "   ATENCAO !!! " +CHR(13)+CHR(13)+;
			" O Faturamento Anterior Gerou a Capa do REGISTRO DE CUPOM "+;
			" [NF.] Numero "+LSnronf+;
			" mas nao Concluiu com a impressao. CANCELE O REGISTRO"+LSnronf+;
			" Para Proceguir."
		ENDIF
		sele orcament
		RETURN
	ENDIF
	sele orcament
	=ECFx_letura()
RETURN


