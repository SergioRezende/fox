	LSalias			= 	ALIAS()

	*--------------------------------------------------------*
	*   As  variaveis LNemp e LDdata sao usadas  em subrotinas do
	* REL206 o que provoca uma falha no controle do relatorio. Para'
	* corrigir os dados sÆo levados para variaveis auxiliares
	*--------------------------------------------------------*

	LNempRef =  LNemp
	LDdtRef  =  LDdata


	SELE empresa
	SET ORDER TO TAG empresa
	SEEK LNemp
    ********************************************************
	LFmapa = .F.
	IF !USED("mapaecf")
		LFmapa = .T.
		IF ! NetUse("mapaecf")
			SELE &LSalias
			return
		ENDIF
	ENDIF
	SELE mapaecf
