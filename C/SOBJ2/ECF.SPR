	SET NEAR ON
	SEEK STR(LNemp,3)+DTOS(LDdata)
	SET NEAR OFF
	IF EOF() OR mapaecf.empresa <> LNemp OR mapaecf.data <> LDdata
		WAIT WINDOW "Nao Existem Dados Com Parametros Informados. <ENTER> "
		SELE &LSalias
		RETURN
	ENDIF
	********************************************************
