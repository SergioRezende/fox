		RETURN
	ENDIF
	
	WAIT WINDOW "AGUARDE. Gerando Arq. Temporario. " NOWAIT
	
	SELE mapaecf
	COPY TO &LSaliastmp ;
    		WHILE mapaecf.empresa = LNempRef AND mapaecf.data = LDdtRef

	SELE 0
	USE &lsaliastmp ALIAS ARQ_GER1

	*******************
