FUNCTION NCFind


	PRIVATE LAno,LMes
	PRIVATE LFreturn

	PRIVATE LSalias
	LSalias		= 	ALIAS()

	=NCVerifyInst()


	SELE &PBTabINCCAlias

	LSchaveAcesso  =NCGetPropVT("CHAVE DE LEITURA")


	DO CASE
		CASE UPPER(LSchaveAcesso) = "MES"
			LAno = NCgetPropVT("ANO")
			LMes = NCgetPropVT("MES")
			SET ORDER TO TAG MES
			SEEK STR(Lano,4)+STR(Lmes,2)
		OTHERWISE
			LAno = NCgetPropVT("ANO")
			LMes = NCgetPropVT("MES")
			SET ORDER TO TAG MES
			SEEK STR(Lano,4)+STR(Lmes,2)
	ENDCASE


	IF FOUND()
		=NCReadProperty()
		=NCSetDerivadas()
	    LFreturn = .T.
	ELSE
	    LFreturn = .f.
	ENDIF
	*--------------------------------------------------------------*
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
