    PRIVATE LSPathOrigem


   	PRIVATE LSFDocXML,LSFItemXML,LSCobrXML,LSTransXML
   	PRIVATE LSFDocLongoXML,LSFItemLongoXML,LSCobrLongoXML,LSTransLongoXML


    PRIVATE LSTime

    PRIVATE LSPathDestino

    LSTime =     CHRTRAN(TIME(),":","")




    *------------------------------------------
	PRIVATE LSalias
	PRIVATE PrmAlsDF

	LSalias = ALIAS()
	PrmAlsDF = DFGetAlias()


	SELE &PrmAlsDF

	GO BOTT
	GO TOP

	SELE &PrmAlsDF
	SET ORDER TO TAG DOCFISCAL
	SEEK STR(PrmEmpresa,3)+PrmMOD_DOC+;
	     PrmCOD_IDFORN+STR(PrmNro_Doc,7)+PrmSERIE_DOC
	
    IF !FOUND()
 	   IF !EMPTY(LSalias) AND USED(LSalias)
		  SELECT &LSalias
	   ENDIF
       RETURN(.F.)
    ENDIF



    *------------------------------------------
    * NOME CAPA
    *------------------------------------------


   	LSFDocXML= DFNmFileXMLDocFiscal(PrmEmpresa,PrmMod_Doc,PrmNro_Doc)
   	LSFDocLongoXML=;
   	     DFNmLongoXMLDocFiscal(PrmEmpresa,PrmMod_Doc+LSTime,PrmNro_Doc)

	IF !FILE("C:\NFE\TMP\"+LSFDocXML)
 	   IF !EMPTY(LSalias) AND USED(LSalias)
		  SELECT &LSalias
	   ENDIF
	   RETURN(.F.)
	ENDIF


    *------------------------------------------
    * NOME ITEM
    *------------------------------------------


   	LSFItemXML	= DINmFileXMLDocFiscal(PrmEmpresa,PrmMod_Doc,PrmNro_Doc)
   	LSFItemLongoXML=;
   	      DINmLongoFileXMLDocFiscal(PrmEmpresa,PrmMod_Doc+LSTime,PrmNro_Doc)

	IF !FILE("C:\NFE\TMP\"+LSFItemXML)
 	   IF !EMPTY(LSalias) AND USED(LSalias)
		  SELECT &LSalias
	   ENDIF
	   RETURN(.F.)
	ENDIF


