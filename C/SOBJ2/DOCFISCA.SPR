**********************************************************************
* IDENTIFICA DURACAO DO ESTOQUE

FUNCTION IMDura
PARAMETERS PrmSaldo,PrmGiro
Private LNDura
	DO CASE
		CASE PrmGiro = 0
			IF PrmSaldo > 0
				LNDura   = 999
			ELSE
				LNDura   = 0
			ENDIF		
		OTHERWISE
			IF PrmSaldo > 0
				LNDura   = PrmSaldo / PrmGiro
				IF LNDura > 999
					LNDura   = 999
				ENDIF				
			ELSE
				LNDura   = 0
			ENDIF
	ENDCASE
RETURN(LNDura)




