FUNCTION DFGerXMLDocFiscal
PARAMETER PrmEmpresa,;
		  PrmMod_Doc,;
		  PrmCOD_IDFORN,;
		  PrmNro_Doc,;
		  PrmSerie_Doc,;
		  PrmObjetivo,;
		  PrmExibeMsg
		
		

	IF type("PrmExibeMsg") <> "C"
		PrmExibeMsg	 = "SIM"
	ENDIF

		
	*-----------------------------------------------------*
	* PrmObjetivo => indica o objetivo da gera‡Æo do xml pode ser:
	*
	*      "EMISSAO"      => Para EMISSOA - NFe/NFSe/CUPOM
	*      "ESCRITURACAO" => Para registro do documento
	*      "CANCELAMENTO" => Para Cancelar o Documento
	*-----------------------------------------------------*


	IF TYPE("PrmObjetivo") <> "C" ;
	   OR !(PrmObjetivo $ "ESCRITURACAO/EMISSAO/CANCELAMENTO/CONSULTAR" )
		  PrmObjetivo = "ESCRITURACAO"
	ENDIF
	

	PRIVATE LFlagOK


	PRIVATE LSalias
	PRIVATE PrmAlsDF

	LSalias = ALIAS()
	PrmAlsDF = DFGetAlias()


	*-------------------------------------------------------*

	=DFModeloRefXML()  && VERIFICA ATUALIZACAO DO LAYOUT


	*-------------------------------------------------------*

   	PRIVATE LSfileXML,LNroIDfileXML


   	LSfileXML	= DFNmFileXMLDocFiscal(PrmEmpresa,PrmMod_doc,PrmNro_Doc)

   	LSfileXML	= "C:\NFE\TMP\"+LSfileXML


  	LNroIDfileXML 		=fcreate(LSfileXML)
   	if LNroIDfileXML<0
      =fclose(LNroIDfileXML)
	  IF PrmExibeMsg	 = "SIM"
      	wait 'ERRO cria‡Æo arquivo tempor rio '+;
      	     LSfileXML+'- <ENTER> ' window
	  ENDIF
      return(.F.)
   	endif





	SELE &PrmAlsDF

	*---------------------------------------------------*
	GO BOTT
	GO TOP


	SELE &PrmAlsDF
	SET ORDER TO TAG DOCFISCAL
	SET NEAR ON
	SEEK STR(PrmEmpresa,3)+PrmMOD_DOC+;
	     PrmCOD_IDFORN+STR(PrmNro_Doc,7)+PrmSERIE_DOC
	
	SET NEAR OFF


	IF !FOUND()
      =fclose(LNroIDfileXML)
	
	 * IF PrmExibeMsg	 = "SIM"
	 *     wait 'Nao foi encontrado Documento em DOCFISCA '+;
     *	       'para gerar XML- <ENTER> ' window
	 * ENDIF

      return(.F.)

   	endif

	*----------------------------------------------------*
	DO RELINIC.PRG          && INICIA VAR P/ TERMOMETRO

	LFsegue = .t.

	LNregistro = RECNO()
	*LNimpressao = RECCOUNT()

	COUNT  WHILE !EOF() ;
	        AND PrmEmpresa    = &PrmAlsDF .empresa;
	        AND PrmMod_Doc    = &PrmAlsDF .MOD_DOC ;
	        AND PrmCOD_IDFORN = &PrmAlsDF .COD_IDFORN ;
	        AND PrmNro_Doc       = &PrmAlsDF .NRO_DOC ;
	        AND PrmSERIE_DOC  = &PrmAlsDF .SERIE_DOC;
         TO LNimpressao
	GO LNregistro
	LNimpressos = 0
	*----------------------------------------------------*


	=DFInicioXML(LSfileXML,LNroIDfileXML)

	SET POINT TO ','



    LFlagOK = .t.

	SELE &PrmAlsDF

    PRIVATE DFsegue
	DFsegue = .t.

	DO WHILE !EOF() ;
			AND	LFsegue = .t. ;
			AND	DFsegue = .t. ;
	        AND PrmEmpresa    = &PrmAlsDF .empresa;
	        AND PrmMod_Doc    = &PrmAlsDF .MOD_DOC ;
	        AND PrmCOD_IDFORN = &PrmAlsDF .COD_IDFORN ;
	        AND PrmNro_Doc    = &PrmAlsDF .NRO_DOC ;
	        AND PrmSERIE_DOC  = &PrmAlsDF .SERIE_DOC

		=UPtermo()



        *-------------------------------------------------------*
        * GERA XML DUPLICATAS XMLCOBR PARA REGISTRO DOC FISCAIS COBRANCA
        *-------------------------------------------------------*
		=W_DEFPROC("DUPLICAT.SPR")
		IF !CRGerXMLDpDocFiscal(;
				&PrmAlsDF .Empresa,;
				&PrmAlsDF .ORCAMENTO,;
				&PrmAlsDF .NRO_DOC,;
				&PrmAlsDF .Mod_doc,;
				&PrmAlsDF .Serie_Doc,;
				&PrmAlsDF .TP_MOV)
				DFsegue = .f.
			    LFlagOK = .f.  && FALHA NO PROCESSO
		    	LOOP
		ENDIF





		IF !DIGerXMLDocFiscal(;
				&PrmAlsDF .Empresa,;
				&PrmAlsDF .Mod_doc,;
				&PrmAlsDF .Cod_IDForn,;
				&PrmAlsDF .Nro_Doc,;
				&PrmAlsDF .Serie_Doc)

			DFsegue = .f.
		    LFlagOK = .f.  && FALHA NO PROCESSO
		    LOOP
		ENDIF




		IF !DFGravaXMLDocFiscal(;
				&PrmAlsDF .Empresa,;
				&PrmAlsDF .Mod_doc,;
				&PrmAlsDF .Cod_IDForn,;
				&PrmAlsDF .Nro_Doc,;
				&PrmAlsDF .Serie_Doc,;
				LSfileXML,;
				LNroIDfileXML,;
				PrmObjetivo)
			DFsegue = .f.
		    LFlagOK = .f.  && FALHA NO PROCESSO
		    LOOP
		ENDIF


		*-------------------------------------------------------*
		* A funcao TRGerXMLTransFiscal disposiociona a tabela
	    * &PrmAlsDF e provoca efeito colateral na rotina
	    * para evitar o efeito marca-se o registro para reposicionar
	    *-------------------------------------------------------*  		

		SELE &PrmAlsDF
		LNregTrspt = RECNO()


		=W_DEFPROC("TRANSPRT.SPR")
		IF !TRGerXMLTransFiscal(;
				&PrmAlsDF .Empresa,;
				&PrmAlsDF .Mod_doc,;
				&PrmAlsDF .Cod_IDForn,;
				&PrmAlsDF .Nro_Doc,;
				&PrmAlsDF .Serie_Doc,;
				&PrmAlsDF .TP_MOV)
				DFsegue = .f.
		    	LFlagOK = .f.  && FALHA NO PROCESSO

				SELE &PrmAlsDF
				go LNregTrspt

			    LOOP
		ENDIF

		SELE &PrmAlsDF
		go LNregTrspt




        *-------------------------------------------------------*
        * GERA XML DUPLICATAS XMLDUPL PARA REGISTRO FINANCEIRO
        *-------------------------------------------------------*
   	    *-----------------------------------------------------*
	    * PrmObjetivo => indica o objetivo da gera‡Æo do xml pode ser:
	    *
	    *      "EMISSAO"      => Para EMISSOA - NFe/NFSe/CUPOM
	    *      "ESCRITURACAO" => Para registro do documento
	    *      "CANCELAMENTO" => Para Cancelar o Documento
	    *-----------------------------------------------------*

		=W_DEFPROC("EMPRESA.SPR")
	    LSemi_Fncr = EMGetFieldValue(PrmEmpresa,"EMITE_FNCR")
        IF LSemi_Fncr = "S"


 	       PRIVATE LSdpMotivo
 	
 	        DO CASE
    	    	CASE PrmObjetivo = "EMISSAO"
	        	   LSdpMotivo   = "IMPRIMIR"

    	    	CASE PrmObjetivo = "ESCRITURACAO"
	        	   LSdpMotivo   = "REGISTRAR"

    	    	CASE PrmObjetivo = "CANCELAMENTO"
	        	   LSdpMotivo   = "CANCELAR"
 	        ENDCASE
 	
*-------------------------------------
*    	    IF PrmObjetivo $ "EMISSAO/ESCRITURACAO"
*        	   LSdpMotivo   = "REGISTRAR"
*	        ELSE
*    	       LSdpMotivo   = "CANCELAR"
*        	ENDIF
*-------------------------------------

			=W_DEFPROC("DUPLICAT.SPR")
			IF !CRGer2XMLDpDocFiscal(;
				&PrmAlsDF .Empresa,;
				&PrmAlsDF .ORCAMENTO,;
				&PrmAlsDF .NRO_DOC,;
				&PrmAlsDF .Mod_doc,;
				&PrmAlsDF .Serie_Doc,;
				&PrmAlsDF .TP_MOV,;
				&PrmAlsDF .NRO_NOTA,;
				LSdpMotivo)
				DFsegue = .f.
			    LFlagOK = .f.  && FALHA NO PROCESSO
		    	LOOP
			ENDIF
		ENDIF
        *-------------------------------------------------------*
		SELE &PrmAlsDF
		go LNregTrspt


		SKIP
	ENDDO





	=DFFinalXML(LSfileXML,LNroIDfileXML)


	SET POINT TO

	DO RELFIM.PRG          && FIM VAR P/ TERMOMETRO
	*-----------------------------------------------*
    =fclose(LNroIDfileXML)
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF

	
RETURN(LFlagOK)


