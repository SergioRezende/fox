    LsTipo  = "T"
   	LsQtde  =  strtran(str(LsQtde,4)," ","0")
   	LSEspaco= space(240)

    LStotal =   LsTipo+LsQtde+Lsespaco
	APPEND BLANK
	REPLACE dms with  LStotal
	
