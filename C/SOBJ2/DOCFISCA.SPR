    *------------------------------------------

	=W_DEFPROC("DUPLICAT.SPR")
   	LSCobrXML	= CRNmFileXMLDocFiscal(PrmEmpresa,PrmMod_Doc,PrmNro_Doc)
   	LSCobrLongoXML	= ;
          CRNmLongoFileXMLDocFiscal(PrmEmpresa,PrmMod_Doc+LSTime,PrmNro_Doc)

	IF !FILE("C:\NFE\TMP\"+LSCobrXML)
 	   IF !EMPTY(LSalias) AND USED(LSalias)
		  SELECT &LSalias
	   ENDIF
	   RETURN(.F.)
	ENDIF

    *------------------------------------------
    * NOME DUPLICATA PARA  FNCR - FINANCEIRO
    *------------------------------------------
    PRIVATE LFTemFinanceiro

	=W_DEFPROC("DUPLICAT.SPR")
    LFTemFinanceiro=CRDFis_Tem_Duplicata(PrmEmpresa, &PrmAlsDF .Orcamento )


	=W_DEFPROC("EMPRESA.SPR")
    LSemi_Fncr = EMGetFieldValue(PrmEmpresa,"EMITE_FNCR")



    IF LFTemFinanceiro = .t. AND LSemi_Fncr = "S"
		=W_DEFPROC("DUPLICAT.SPR")
	   	LSFncrXML= CRNm2FileXMLDocFiscal(PrmEmpresa,PrmMod_Doc,PrmNro_Doc)
   	    LSFncrLongoXML	= ;
         CRNm2LongoFileXMLDocFiscal(PrmEmpresa,PrmMod_Doc+LSTime,PrmNro_Doc)

	ENDIF



    *------------------------------------------
    * NOME TRANSPORTADORA
    *------------------------------------------

	=W_DEFPROC("TRANSPRT.SPR")
   	LSTransXML	= TRNmFileXMLDocFiscal(PrmEmpresa,PrmMod_Doc,PrmNro_Doc)
   	LSTransLongoXML	=;
   	      TRNmLongoFileXMLDocFiscal(PrmEmpresa,PrmMod_Doc+LSTime,PrmNro_Doc)

	IF !FILE("C:\NFE\TMP\"+LSTransXML)
 	   IF !EMPTY(LSalias) AND USED(LSalias)
		  SELECT &LSalias
	   ENDIF
	   RETURN(.F.)
	ENDIF


	*--------------------------------------------------*

	=W_DEFPROC("PARAMETR.SPR")

    IF "2D" $ PrmMod_Doc
 		LSPathDestino   = PMGetFieldValue(PrmEmpresa,"DIR_APPTEF")
    ELSE
 		LSPathDestino   = PMGetFieldValue(PrmEmpresa,"DIR_DFIS")
	ENDIF


    IF TYPE("LSPathDestino") <> "C" or EMPTY(LSPathDestino)
         LSPathDestino = "C:\NFE\"
	else
         LSPathDestino = ALLTRIM(LSPathDestino)
    ENDIF
    LSPathBKP = ALLTRIM(LSPathDestino)+"\BKP\"


	*--------------------------------------------------*
	LSFileOrigem     = "C:\NFE\TMP\"+LSFDocXML
	LSFileDestino    = LSPathDestino+LSFDocLongoXML
	LSFileBKP        = LSPathBKP+LSFDocLongoXML

			
	=DFCOPYLongo(LSFileOrigem,LSFileDestino)
	=DFCOPYLongo(LSFileOrigem,LSFileBKP)


	IF FILE(LSFileOrigem)
		DELETE FILE &LSFileOrigem
	ENDIF

	*--------------------------------------------------*

	LSFileOrigem     = "C:\NFE\TMP\"+LSFItemXML
	LSFileDestino    = LSPathDestino+LSFItemLongoXML
	LSFileBKP        = LSPathBKP+LSFItemLongoXML
			
	=DFCOPYLongo(LSFileOrigem,LSFileDestino)
	=DFCOPYLongo(LSFileOrigem,LSFileBKP)


