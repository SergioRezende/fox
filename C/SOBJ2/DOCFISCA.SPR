FUNCTION IMFaxinaDados

	PRIVATE LSAlias, TOTAL, CTR
	=IMVerifyInst()

	LSAlias = IMGetAlias()
	
	SELE &LSAlias

	COUNT TO TOTAL
	CTR = 0

	GO TOP
	DO WHILE !EOF()
		=RLOCK()


		CTR = 	CTR + 1
		MSG = STR(CTR,7)+" DE "+STR(TOTAL,7)
		WAIT WINDOW MSG NOWAIT


