			RtnSerie_Doc AS DF_SERIE,;
			0            AS DF_VLR_DOC,;
			RtnDocEletro AS DF_ELETRO, ;
	        LP(IT.CFO)   AS IT_CFOP,;
	        IT.MOVESTQ   AS IT_MOVESTQ,;
	        IT.OPERACAO  AS IT_OPERAC,;
	        IT.CODIGO    AS IT_CODIGO,;
	        IT.QTDE      AS IT_QTDE,;
	        IT.VLRVENDA  AS IT_VLR;
		FROM ;
		     &ALSEmp EMP, &ALSItem IT  ;
		  WHERE;
