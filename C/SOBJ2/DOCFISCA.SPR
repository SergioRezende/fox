FUNCTION NFAjstNroNF
PARAMETERS PrmEmpresa


	SELE empresa
	SEEK PrmEmpresa
	IF !REGLOCK(.T.)
	   WAIT WINDOW ;
	   "Registro Empresa nao Pode ser Bloq. Tente Novamente.<ENTER>"
	   SELE orcament
	   RETURN(.f.)
	ENDIF


	SET NEAR ON

		
    *-----------------------------------------*
	* AJUSTA NUMERO CUPOM
    *-----------------------------------------*
	SELE nota
	SET ORDER TO TAG nota
	SEEK STR(PrmEmpresa,3)+STR(2000000,7)
    skip -1
	
	IF PrmEmpresa = NOTA.EMPRESA ;
		AND ;
		nota.nota > 1000000 and nota.nota < 2000000
		
		SELE empresa 			&& DEVE ESTA BLOQ. FUNCINA COM SINALEIRO
		REPLACE empresa.ult_cupom 	WITH nota.nota - 1000000
		REPLACE empresa.flagimpcpm 	WITH 4
	ENDIF


    *-----------------------------------------*
	* AJUSTA NUMERO NFE
    *-----------------------------------------*
	SELE nota
	SET ORDER TO TAG nota
	SEEK STR(PrmEmpresa,3)+STR(4000000,7)
    skip -1
	
	IF PrmEmpresa = NOTA.EMPRESA ;
		AND ;
		nota.nota > 3000000 and nota.nota < 4000000
		
		SELE empresa 			    && DEVE ESTA BLOQ. FUNCINA COM SINALEIRO
		REPLACE empresa.nota 	    WITH nota.nota - 3000000
		REPLACE empresa.flagimpnf 	WITH 4

	ENDIF



    *-----------------------------------------*
	* AJUSTA NUMERO NFE
    *-----------------------------------------*
	SELE nota
	SET ORDER TO TAG nota
	SEEK STR(PrmEmpresa,3)+STR(5000000,7)
    skip -1
	
	IF PrmEmpresa = NOTA.EMPRESA ;
		AND ;
		nota.nota > 4000000 and nota.nota < 5000000
		
		SELE empresa 			&& DEVE ESTA BLOQ. FUNCINA COM SINALEIRO
		REPLACE empresa.ult_nfsrvc 	WITH nota.nota - 4000000
		REPLACE empresa.flagimpsrv 	WITH 4
	ENDIF


	SET NEAR OFF

    UNLOCK ALL
	SELE orcament
	RETURN(.T.)

    ****************************************************************
RETURN(.F.)


