FUNCTION CPCreate
	=W_DEFPROC("rotinas.spr")
	PRIVATE LSalias
	PRIVATE LSAlsTmp
    PRIVATE LMaxDim
	LSalias		= 	ALIAS()
	
	IF TYPE("PBCupomAlias") <> "U" ;
	   		AND !EMPTY(PBCupomAlias) ;
	   		AND USED(PBCupomAlias)
			RETURN(.T.)
	ENDIF


	PUBLIC PBCupomAlias

	IF USED("Nota")
	     =NetArqAgain("Nota")
	     PBCupomAlias     = Alias()
	ELSE
	     =NetArqAgain("Nota")
	     LSAlsTmp     = Alias()
	     =NetArqAgain("Nota")
	     PBCupomAlias     = Alias()
		 IF !EMPTY(LSAlsTmp) AND USED(LSAlsTmp)
			SELECT &LSAlsTmp
			USE
		 ENDIF
	ENDIF	

    SELE &PBCupomAlias
	LMaxDim = Fcount()
    PUBLIC DIMENSION VTCupom[LMaxDim]
    PUBLIC DIMENSION VDCupom[2,3]			&& VETOR PARA PROPRIEDADES
    PUBLIC DIMENSION VFCupom[1]      && VETOR CABECALHO DOS CAMPOS
	=AFIELDS(VFCupom)
	*-----------------------------------------------------------*
	=CPReadProperty()
	=CPSetDerivadas()
	*--------------------------------------------------------------*
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF
RETURN



