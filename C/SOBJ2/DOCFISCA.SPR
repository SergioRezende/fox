	SELECT TMPGRO1.*, ;
		IMDura((sld_gyn+sld_DPO),(gro_gyn+gro_DPO))  AS dura_gyn,;
		IMDura((sld_sia+sld_CNB),(gro_sia+gro_CNB))  AS dura_sia,;
