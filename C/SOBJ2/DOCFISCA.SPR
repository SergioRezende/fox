FUNCTION DICreate
	=W_DEFPROC("rotinas.spr")
	PRIVATE LSalias
	PRIVATE LSAlsTmp
    PRIVATE LMaxDim
	LSalias		= 	ALIAS()
	
	IF TYPE("PBDocFisItAlias") <> "U" ;
	   		AND !EMPTY(PBDocFisItAlias) ;
	   		AND USED(PBDocFisItAlias)
