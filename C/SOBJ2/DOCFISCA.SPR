FUNCTION IMPosicItemmov
PARAMETERS PrmLoja,PrmCodigo,PrmData

	*------------------------------------------------------------------*
	SELE &PBItemmovAlias
	SET ORDER TO  TAG movimento
	SET NEAR ON
	SEEK STR(PrmLoja,3)+PrmCodigo+DTOS(PrmData+1)
	SET NEAR OFF

	IF  &PBItemmovAlias .data <> PrmDt_Final OR  ;
