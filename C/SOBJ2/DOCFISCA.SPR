*		   RtnChvNFe,;
*		   RtnJstfCanc,;
*		   RtnPrtcCanc

PARAMETER  PrmEmpresa,;
		   PrmMod_Doc,;
		   PrmCOD_IDFORN,;
		   PrmNro_Doc,;
		   PrmSerie_Doc,;
		   PrmEsperaResposta,;
		   RtnStatus,;
		   RtnRecibo,;
		   RtnProtocolo,;
		   RtnChvNFe,;
		   RtnJstfCanc,;
		   RtnPrtcCanc

	*-------------------------------------------------------*

	PRIVATE LSalias
	PRIVATE PrmAlsDF

	LSalias = ALIAS()
	PrmAlsDF = DFGetAlias()

	SELE &PrmAlsDF

	GO BOTT
	GO TOP

	SELE &PrmAlsDF
	SET ORDER TO TAG DOCFISCAL
	SEEK STR(PrmEmpresa,3)+PrmMOD_DOC+;
	     PrmCOD_IDFORN+STR(PrmNro_Doc,7)+PrmSERIE_DOC
	
    IF !FOUND()
 	   IF !EMPTY(LSalias) AND USED(LSalias)
		  SELECT &LSalias
	   ENDIF
       LSstatus = "ERRO ! Nao encontrado documento fiscal"
       RETURN(LSstatus)
    ENDIF
		
		
		
		
	*-------------------------------------------------------*
	PRIVATE LSstatus,LSstatus2
	LSstatus = ""
    LSstatus2 = ""

	IF PrmEsperaResposta = "ESPERA"
		LSstatus=DFEsperaRspNFe(PrmEmpresa,PrmMod_Doc,PrmNro_Doc)
	ELSE
		LSstatus=DFBuscaArqRspNFe(PrmEmpresa,PrmMod_Doc,PrmNro_Doc)
	ENDIF


	*-------------------------------------------------------*


	DO CASE

		CASE LSstatus = "ERRO ! Nao houve retorno do comando"
				RtnStatus = LSstatus
		OTHERWISE
				RtnCodStatus = SUBS(LSstatus,1,AT("|",LSstatus,1)-1)
				RtnCodStatus = CHRTRAN(RtnCodStatus,"|","")


                * Status + descricao
				*----------------------------------------*				
				RtnStatus   = SUBS(LSstatus,;
				 			   AT("|",LSstatus,1)+1,;
				               AT("|",LSstatus,2)-AT("|",LSstatus,1))
				RtnStatus = CHRTRAN(RtnStatus,"|","")

                * NUMERO NOTA
				*----------------------------------------*				
				RtnNota   = SUBS(LSstatus,;
				 			   AT("|",LSstatus,2)+1,;
				               AT("|",LSstatus,3)-AT("|",LSstatus,2))
				RtnNota = CHRTRAN(RtnNota,"|","")

                *  RECIBOU OU NUMERO COO
				*----------------------------------------*				
				RtnRecibo = SUBS(LSstatus,;
				 			   AT("|",LSstatus,3)+1,;
				               AT("|",LSstatus,4)-AT("|",LSstatus,3))
				RtnRecibo = CHRTRAN(RtnRecibo,"|","")

                * NUMERO PROTOCOLO OU ECF
				*----------------------------------------*				
				RtnProtocolo = SUBS(LSstatus,;
				 			   AT("|",LSstatus,4)+1,;
				               AT("|",LSstatus,5)-AT("|",LSstatus,4))
				RtnProtocolo = CHRTRAN(RtnProtocolo,"|","")


                * CHAVE NFE OU SERIE ECF
				*----------------------------------------*				
				RtnChvNFe    = SUBS(LSstatus,;
				 			   AT("|",LSstatus,5)+1,;
				               AT("|",LSstatus,6)-AT("|",LSstatus,5))
				RtnChvNFe = CHRTRAN(RtnChvNFe,"|","")

                * JUSTIFICATIVA OU ECF CAIXA
 				*----------------------------------------*				
				RtnJstfCanc   = SUBS(LSstatus,;
				 			   AT("|",LSstatus,6)+1,;
				               AT("|",LSstatus,7)-AT("|",LSstatus,6))
				RtnJstfCanc = CHRTRAN(RtnJstfCanc,"|","")
				*----------------------------------------*				
				RtnPrtcCanc   = SUBS(LSstatus,;
				 			   AT("|",LSstatus,7)+1,;
				               AT("|",LSstatus,8)-AT("|",LSstatus,7))
				RtnPrtcCanc = CHRTRAN(RtnPrtcCanc,"|","")
				*----------------------------------------*				
				IF RtnPrtcCanc = ""
					RtnJstfCanc = ""
				ENDIF
	
		ENDCASE


        *------------------------------------------
        * NOME DUPLICATA PARA  FNCR - FINANCEIRO
        *------------------------------------------

        *--------------------------------------------------------------
        * em 02/01/2013 desativei a espera por resposta do financeiro
        * por estar atrasando a emissÆo de nota. Rever o processo
        *--------------------------------------------------------------

*        PRIVATE LFTemFinanceiro
*	    =W_DEFPROC("DUPLICAT.SPR")
*        LFTemFinanceiro=;
*             CRDFis_Tem_Duplicata(PrmEmpresa, &PrmAlsDF .Orcamento )
*
*		=W_DEFPROC("EMPRESA.SPR")
*	    LSemi_Fncr = EMGetFieldValue(PrmEmpresa,"EMITE_FNCR")
*
*
*       IF LSemi_Fncr = "S" and LFTemFinanceiro = .t.
*			IF PrmEsperaResposta = "ESPERA"
*				LSstatus2=;
*				 DFFncrEsperaRsp(PrmEmpresa,PrmMod_Doc,PrmNro_Doc)
*			ELSE
*				LSstatus2=;
*				 DFFncrBuscaRsp(PrmEmpresa,PrmMod_Doc,PrmNro_Doc)
*			ENDIF
*       ENDIF

 	   IF !EMPTY(LSalias) AND USED(LSalias)
		  SELECT &LSalias
	   ENDIF


RETURN(LSstatus+LSstatus2)


