	   totproduto,;
 	   aliq_iss,;
 	   vlr_iss;
 	from TMPNOTA where empresa = PrmEmpresa;
       and nota > 1000000 ;
	   and nota < 2000000 ;
       and data <=  PrmDataFinal ;
       and data >= PrmDataInicio and vlr_iss > 0;
       and STR(EMPRESA,3)+STR(NOTA,7) ;
           not in (SELECT NRO_CUPOM from DMS_CPMs);
    into cursor DMS_A3



    SELECT * FROM  DMS_A1 ;
    UNION ;
    SELECT * FROM  DMS_A2 ;
    into cursor DMS_A4


    SELECT * FROM  DMS_A3 ;
    UNION ;
    SELECT * FROM  DMS_A4 ;
    into cursor DMS_A5


    SELECT * FROM  DMS_A5 ;
 	    order by NOTA into cursor DMS_A1

	SET TALK OFF
    *-----------------------------------------------------------*
	
	sele TMPNOTA
	USE

	sele DMS_A2
	USE

	sele DMS_A3
	USE

	sele DMS_A4
	USE

	sele DMS_A5
