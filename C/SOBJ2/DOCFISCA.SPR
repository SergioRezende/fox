	              	);
	              );
   	INTO CURSOR TMPG_1




	SELECT GRP.ORDEM,GRP.CODIGO ,GRP.CLASSIFICA,GRP.DESCRICAO,;
	1.000*IMgiroTendenciaLJ(11,GRP.CODIGO,DATE()-100,DATE()) AS GRO_DPO,;
		IMGetSaldo(11,GRP.Codigo, DATE()) AS SLD_DPO, ;
