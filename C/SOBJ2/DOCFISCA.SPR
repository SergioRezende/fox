	LSFileDestino    = LSPathDestino+LSCobrLongoXML
	LSFileBKP        = LSPathBKP+LSCobrLongoXML
			
	=DFCOPYLongo(LSFileOrigem,LSFileDestino)
	=DFCOPYLongo(LSFileOrigem,LSFileBKP)


	IF FILE(LSFileOrigem)
		DELETE FILE &LSFileOrigem
	ENDIF


	*--------------------------------------------------*

	LSFileOrigem     = "C:\NFE\TMP\"+LSTransXML
	LSFileDestino    = LSPathDestino+LSTransLongoXML
	LSFileBKP        = LSPathBKP+LSTransLongoXML
			
	=DFCOPYLongo(LSFileOrigem, LSFileDestino)
	=DFCOPYLongo(LSFileOrigem,LSFileBKP)


	IF FILE(LSFileOrigem)
		DELETE FILE &LSFileOrigem
	ENDIF

	
	*--------------------------------------------------*

	=W_DEFPROC("EMPRESA.SPR")
    LSemi_Fncr = EMGetFieldValue(PrmEmpresa,"EMITE_FNCR")
    IF LSemi_Fncr = "S" and LFTemFinanceiro = .t.

		=W_DEFPROC("PARAMETR.SPR")

		LSPathDestino   = PMGetFieldValue(PrmEmpresa,"DIR_DFIN")


	    IF TYPE("LSPathDestino") <> "C" or EMPTY(LSPathDestino)
    	     LSPathDestino = "C:\DUPL\"
		else
        	 LSPathDestino = ALLTRIM(LSPathDestino)
	    ENDIF
    	LSPathBKP = ALLTRIM(LSPathDestino)+"\BKP\"




  	    =W_DEFPROC("DUPLICAT.SPR")
	    LSPathOrigem  = CRPthTmpDup(PrmEmpresa)


		LSFileOrigem     = ALLTRIM(LSPathOrigem) +LSFncrXML
		
		
		
		LSFileDestino    = LSPathDestino+LSFncrLongoXML
		LSFileBKP        = LSPathBKP+LSFncrLongoXML
			
		=DFCOPYLongo(LSFileOrigem, LSFileDestino)
		=DFCOPYLongo(LSFileOrigem,LSFileBKP)


		IF FILE(LSFileOrigem)
			DELETE FILE &LSFileOrigem
		ENDIF
	ENDIF

	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF


RETURN(.T.)


