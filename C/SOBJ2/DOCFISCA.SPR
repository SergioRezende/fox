	IF !EMPTY(PBDocFiscaAlias) AND USED(PBDocFiscaAlias)
		SELECT &PBDocFiscaAlias
		USE
	ENDIF	
	RELEASE PBDocFiscaAlias
    RELEASE VTDocFisca
    RELEASE VDDocFisca
	RELEASE VFDocFisca
	RELEASE VCDocFisca

	*-----------------------------------------------------------*
	*--------------------------------------------------------------*
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF
