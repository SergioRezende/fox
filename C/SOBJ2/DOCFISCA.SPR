			=fPuts(LNroIDfileXML,LString,LEN(LString))

			LctrSeq = NFselec.nota

		ELSE

			LString = STR(LctrSeq,7) +;
					  " OK "+;
					  " de "+;	
			          DTOC( NFselec.data )

			=fPuts(LNroIDfileXML,LString,LEN(LString))
		ENDIF


		SELE  NFselec
		SKIP

		LctrSeq  = LctrSeq + 1

	ENDDO

	SELE NFselec
	USE

	LString = "Final Verificacao"

	=fPuts(LNroIDfileXML,LString,LEN(LString))

