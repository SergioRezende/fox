	ENDIF	

    SELE &PBDocFisItAlias
	LMaxDim = Fcount()
    PUBLIC DIMENSION VTDocFisIt[LMaxDim]
    PUBLIC DIMENSION VDDocFisIt[2,3]			&& VETOR PARA PROPRIEDADES
    PUBLIC DIMENSION VFDocFisIt[1]      && VETOR CABECALHO DOS CAMPOS
	=AFIELDS(VFDocFisIt)

	*-----------------------------------------------------------*
	=DIReadProperty()
	=DISetDerivadas()
