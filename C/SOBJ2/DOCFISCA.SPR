	FROM &ALSNota NF;
	WHERE   NF.empresa = PrmEmp ;
		AND NF.data >= PrmDtIni ;
    	AND NF.data <= PrmDtFim ;
    INTO CURSOR NFselec;
    ORDER BY NF.NOTA
