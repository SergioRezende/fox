FUNCTION GRFaxinaDados

	PRIVATE LSAlias, TOTAL, CTR
	=GRVerifyInst()

	LSAlias = GRGetAlias()
	
	SELE &LSAlias

	COUNT TO TOTAL
	CTR = 0

	GO TOP
	DO WHILE !EOF()
		=RLOCK()


		CTR = 	CTR + 1
		MSG = STR(CTR,7)+" DE "+STR(TOTAL,7)
		WAIT WINDOW MSG NOWAIT


		REPLACE DESCRICAO WITH ;
		   UPLimpaString(DESCRICAO)

		IF EMPTY(UNIDADE)
			REPLACE UNIDADE  WITH "UN"
		ENDIF
		
		SKIP
	ENDDO
	


RETURN(.t.)


