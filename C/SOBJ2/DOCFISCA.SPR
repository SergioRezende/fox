	*----------------------------------------------------------*
	*----------------------------------------------------------*

	=W_DEFPROC("duplicat.spr")
    LNnroDup = CRNroIDdupl(PrmEmpresa,PrmNota,1)


    SELE &ALS_Duplicat
  	SET ORDER TO TAG doc
	SET NEAR ON
    SEEK STR(PrmEmpresa,3)+STR(LNnroDup,9)
    SET NEAR OFF
	*-------------------------------------------------------------*
	DO WHILE !EOF() ;
