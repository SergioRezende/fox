         ,Q:\SCGC\COMUM\FORNECE FR;
   	WHERE GRP.ORDEM >= PrmOrdIni AND GRP.ORDEM <= PrmOrdFim  ;
			 AND STR(GRP.tp_control,1) $ PrmControl ;
