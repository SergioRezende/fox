FUNCTION ORSleOrFinFat		  && sist. localizacao de orcamentos
PARAMETERS  PrmTipoOperacao   && VENDAS  //  OUTRAS


	UNLOCK ALL


	*------------------------------------------------------------*
	* CLASSIFICACAO:  [*****]
	*------------------------------------------------------------*
	* OBJETIVO....: Visualisar Orcamentos que atendam aos parametros
	*
	*------------------------------------------------------------*
	* COMENTARIO..:
	*------------------------------------------------------------*
	* OBS........ :
	*------------------------------------------------------------*
	*  TABELAS....:
	*------------------------------------------------------------*
	*  PARAMETROS..:
	*		PARAMETERS LSsit, LScompl, LSorigem
	*			LSsit   => indica as situacoes que devem ser relacionadas
	*			LScompl => indica os complementos de situacoes solicitados
	*		 	LSorigem=> indica qual operacao solicitou ex: RESERVA,
	*						LIBERA
	*------------------------------------------------------------*
	*  RETORNO.....:
	*------------------------------------------------------------*
	* EXEMPLO : CHAMADA DO PROGRAMA SCGC2
	*------------------------------------------------------------*
	=W_DEFPROC("rotinas.spr")
	*------------------------------------------------------------------*

	PRIVATE 	LFdestrava

	ON KEY LABEL ESCAPE
	CLEAR TYPEAHEAD
    SELECT orcament




	LNreg = RECNO()     && permite reposicionar registro

	LNemp = orcament.empresa
	LNorca= orcament.orcamento
	SET ORDER TO tag situacao

	SET NEAR ON
	SEEK STR(wp_empresa,3)+"o"
	SET NEAR OFF
	IF EOF() OR wp_empresa <> orcament.empresa OR ;
			!(LEFT(orcament.situacao,1) $ "o")
		wp_msg = "Nao existem orcamentos para esta opcao. <ENTER>"
		=UPbeeps(.F.,wp_msg)
		UNLOCK
	    m.sOldError=ON('error')
		ON ERROR *
		GO LNreg
		ON ERROR &sOldError
		KEYBOARD CHR(27)		
		=INKEY(0)
 		RETURN(.F.)
	ENDIF
************************>
	wl_arqtmp = ""
	LNtmp     = 65		&& TABELA ASCII A = 65
	IF !UPabretmp("orc")
		wp_flgfecha = .t.
	ENDIF
	IF wp_flgfecha
		KEYBOARD "{ESCAPE}"
		RETURN(.F.)
	ENDIF
	SET SAFET OFF
	LStmp = "&wp_dirtmp"+"&wl_arqtmp"
*****************************>

	SELE clienc
	SET ORDER TO TAG emporca

    SELE tipooper
	SET ORDER TO TAG tipo


	SELE orcament

    LFdestrava = .t.
	SET RELATION TO STR(EMPRESA,3)+STR(ORCAMENTO,6) INTO clienc

	SET RELATION TO "S"+ORCAMENT.TIPO INTO tipooper ADDITIVE

    IF PrmTipoOperacao = "VENDAS"	
	   COPY TO &LStmp FIELDS empresa, orcamento,operador,situacao,;
					data,clienc.fone,VLR_ISSRTD,;
					valor,clienc.nome, veiculo,placa,;
					dt_fat,nota,dtregis,hregis,usrregis ;
	   	    FOR ;
                     LEFT(orcament.situacao,1) $ "o" AND  ;
                     tipooper.ch_opera = "1" ;
	    		WHILE wp_empresa = orcament.empresa ;
 		   	             WITH CDX

    ELSE
	   COPY TO &LStmp FIELDS empresa, orcamento,operador,situacao,;
					data,clienc.fone,VLR_ISSRTD,;
					valor,clienc.nome, veiculo,placa,;
					dt_fat,nota,dtregis,hregis,usrregis ;
	   	    FOR ;
                     LEFT(orcament.situacao,1) $ "o" AND  ;
                     tipooper.ch_opera <> "1" ;
	    		WHILE wp_empresa = orcament.empresa ;
 		   	             WITH CDX

    ENDIF 		   	


	SET RELATION TO

    SELE 0
    USE &LStmp EXCLU ALIAS ORCABROW
	INDEX ON NOME TAG nome_todos ADDITIVE

*-----------------------> FIM PREPARACAO ARQ. TEMPORARIO	
	IF EOF() AND BOF()
		wp_msg = "Nao existem orcamentos para esta opcao. <ENTER>"
		=UPbeeps(.F.,wp_msg)
		KEYBOARD CHR(27)		
		=INKEY(0)
	    SELECT orcament
		GO LNreg
	ELSE
		Lpar1 = ""
		Lpar2 = ""
		SET ORDER TO TAG nome_todos
		GO TOP

		DO ORLoc_Browse WITH .T., Lpar1, Lpar2, "B"


	    SELECT orcament
		SET ORDER TO tag orcamento
		SET RELATION TO
*------------------------------------- posiciona orcamento
		SEEK STR(orcabrow.empresa,3)+STR(orcabrow.orcamento,6)
*------------------------------------ encerra secao orcabrow
		IF !FOUND() OR LASTKEY() = 27
			
			KEYBOARD CHR(27)		
			GO LNreg
  	        SELECT orcabrow
   	        USE
	        SELECT orcament
            SCATTER MEMVAR MEMO
            RETURN(.F.)
			
		ENDIF
	ENDIF
	SELECT orcabrow
	USE
	SELECT orcament
    SCATTER MEMVAR MEMO
RETURN(.T.)


