	Lchv_ler    = ORGetPropVT("CHV_LER")
	Lchv_compara= ORGetPropVT("CHV_COMPARA")
	Lchv_brow   = ORGetPropVT("CHV_BROW")

	
	SELE &PBOrcamentAlias
	DO CASE
	
