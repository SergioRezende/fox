FUNCTION CLImportaCad

PARAMETER  PrmEmpresa,;
           PrmEsperaResposta,;
		   RtnStatus
		
	*-------------------------------------------------------*

	PRIVATE LSalias
	PRIVATE PrmAlsCL

	LSalias = ALIAS()
	PrmAlsCL = CLGetAlias()

	SELE &PrmAlsCL

	GO BOTT
	GO TOP

		
		
		
		
	*-------------------------------------------------------*
	PRIVATE LSstatus,LSstatus2
	LSstatus = ""
    LSstatus2 = ""

	IF PrmEsperaResposta = "ESPERA"
		LSstatus=CLEsperaRspCad(PrmEmpresa)
	ELSE
