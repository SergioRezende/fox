	*------------------------------------------*

    PRIVATE LSAlsPeriodo
	PRIVATE ;
	    PrmEmp,PrmBanco,PrmCarteira,PrmVariacao,PrmAviso


	*------------------------------------------------------------*
	=W_DEFPROC("rotinas.spr")
	*------------------------------------------*
	PRIVATE LSalias
