	=UP_fecha("duplicat" ,LFduplicat)
	=UP_fecha("retornbc" ,LFretornbc)
	=UP_fecha("retornmv" ,LFretornmv)
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF
RETURN

