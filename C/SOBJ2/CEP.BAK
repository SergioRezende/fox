FUNCTION CPFind

	PRIVATE LCEP
	PRIVATE LFreturn

	PRIVATE LSalias
	LSalias		= 	ALIAS()

	=CPVerifyInst()


	LCEP   = CPGetPropVT("ENDE_CEP")


	SELE &PBCepAlias
	SET ORDER TO TAG ende_cep
	SEEK LCEP
	IF FOUND()
		=CPReadProperty()
		=CPSetDerivadas()
	    LFreturn = .T.
	ELSE
	    LFreturn = .f.
	ENDIF
	*--------------------------------------------------------------*
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF
RETURN(LFreturn)



