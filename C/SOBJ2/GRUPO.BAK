FUNCTION GRFind
	PRIVATE LCodigo
	PRIVATE LClassifica
	PRIVATE LFreturn
	PRIVATE LSchaveAcesso

	PRIVATE LSalias
	LSalias		= 	ALIAS()

	=GRVerifyInst()

	SELE &PBGrupoAlias
	LSchaveAcesso  =GRGetPropVT("CHAVE DE LEITURA")

	DO CASE
		CASE UPPER(LSchaveAcesso) = "CLASSIFICA"
			LClassifica    = GRGetPropVT("CLASSIFICA")
			SET ORDER TO TAG CLASSIFICA
			SEEK LClassifica
		OTHERWISE
			LCodigo     = GRGetPropVT("CODIGO")
			SET ORDER TO TAG CODIGO
			SEEK LCodigo

	ENDCASE


	IF FOUND()
		=GRReadProperty()
		=GRSetDerivadas()   && carga dos calculos derivados
	    LFreturn = .T.
	ELSE

	    LFreturn = .f.
	ENDIF
	*--------------------------------------------------------------*
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF
RETURN(LFreturn)




