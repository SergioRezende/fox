		SELECT &PBGrupoAlias
		USE
	ENDIF	
	RELEASE PBGrupoAlias
    RELEASE VTgrupo
    RELEASE VDGrupo
    RELEASE VFGrupo
    RELEASE VCGrupo

	*-----------------------------------------------------------*
	*--------------------------------------------------------------*
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF
RETURN(.T.)


