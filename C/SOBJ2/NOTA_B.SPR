	STORE 0  TO LNNFCop, LNnfSrvCop
	

	STORE 0  TO LNcupom
	STORE "" TO LStp_process
	STORE {} TO datanf
	STORE "" TO horanf,LsOrientacao
    STORE .F. TO LFGerNF,LFGerNFS


