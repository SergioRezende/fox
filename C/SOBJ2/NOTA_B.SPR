				    (orcament.empresa),(orcament.orcamento)

		CASE dt_fat >= {10.03.2008} AND ;
			(orcament.empresa = 01 or orcament.empresa = 02)		
				DO  CRgeraDE12MAR08 WITH ;
				    (orcament.empresa),(orcament.orcamento)
