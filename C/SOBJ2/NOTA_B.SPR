FUNCTION NFchqLiberCred
PARAMETERS PrmEmpresa,PrmOrcamento


		*---------------------------------------------------*

		LSl = LEFT(orcament.situacao,1)
		LSr = RIGHT(orcament.situacao,1)



		IF  LSr $ "Do" AND (orcament.tp_parcela > 1)
			WAIT WINDOW ;
