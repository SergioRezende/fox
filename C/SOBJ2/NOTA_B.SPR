	SET RELATION TO
	SET POINT TO
	SET SEPARATOR  TO
	*--------------------------------------------------------------*
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF
RETURN(LFretorno)






