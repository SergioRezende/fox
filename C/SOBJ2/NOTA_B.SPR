			IF ORLerRegistro(LNemp,nota.referencia)
  	    	
	  	    	=ORSetPropVT("NFE_STATUS",RtnStatus)
				=ORSetPropVT("NFE_RECIBO",RtnRecibo)
				=ORSetPropVT("NFE_PROTOC",RtnProtocolo)
				=ORSetPropVT("NFE_CHV",RtnChvNFe)
