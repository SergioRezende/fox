				m.codigo  = itemmov.codigo
		
				IF itemmov.movestq = "S"
					m.qtde    = itemmov.qtde
				ELSE
					m.qtde    = 0
				ENDIF

				m.classifica = itemmov.classifica
				m.motivo    = 1		&& 1 => comissao normal sobre venda
				m.vendedor  = m.servidor
				m.descricao = itemmov.descricao
