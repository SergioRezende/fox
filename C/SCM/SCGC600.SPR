FUNCTION _2lx0oqy13     &&  m.nivel VALID
#REGION 1
SELECT funcao
SET ORDER TO TAG nivel
IF LASTKEY() = 9
   ON KEY LABEL ESCAPE
   DO loc_dlog
	ON KEY LABEL ESCAPE KEYBOARD "{BACKTAB}"
   SCATTER MEMVAR FIELDS nivel MEMO
   IF LASTKEY() = 27
	   SELECT prod_cms
	   RETURN .F.
   ENDIF
ELSE
   IF m.nivel > 0	
	   IF !SEEK(m.nivel)
		   SELECT prod_cms
		   RETURN .F.
	   ENDIF
   ENDIF
ENDIF
IF m.nivel = 0
	m.nmfuncao = "PADRAO"
ELSE
	m.nmfuncao = funcao.descricao
ENDIF
SHOW GET m.nmfuncao

SELECT prod_cms
SET ORDER TO TAG tabela
SEEK STR(wp_empresa,3)+STR(m.tabela,3)+m.serie+m.codigo+STR(m.nivel,2)

RETURN(UPtratachv())


