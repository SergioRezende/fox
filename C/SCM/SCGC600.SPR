PROCEDURE BTN_VAL1

	PARAMETER tecla, m.chv_ler, m.chv_compara, m.chv_brow
	SELECT prod_cms

    do btn_val with tecla, m.chv_ler, m.chv_compara, m.chv_brow

	IF m.nivel <> 0 OR isediting
		SHOW GET item_btn DISABLE
	ENDIF


	SELECT prod_cms
    m.tabpreco = cadtab.tabela
   	m.serie    = cadtab.serie
   	m.empresa  = wp_empresa
	
	SELECT funcao
	SET ORDER TO TAG nivel
    SEEK m.nivel
	IF FOUND()
			m.nmfuncao = funcao.descricao
	ELSE
		m.nmfuncao = "  "
		IF m.nivel = 0
			m.nmfuncao = "PADRAO"
		ENDIF
	ENDIF	
	SHOW GET m.nmfuncao
	if tecla $ 'ADD/EDIT'
        return
    endif
	SELE grup_cms
	SET ORDER TO TAG grupo
	SEEK m.codigo
	SHOW GET grup_cms.descricao
	SELECT prod_cms


RETURN




