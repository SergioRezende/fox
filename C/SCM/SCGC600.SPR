FUNCTION _2lx0oqy12     &&  m.codigo VALID
#REGION 1
SELE grup_cms
SET ORDER TO TAG grupo
IF LASTKEY() = 9
   ON KEY LABEL ESCAPE
   DO loc_dlog
   ON KEY LABEL ESCAPE KEYBOARD "{BACKTAB}"
   m.codigo =  grup_cms.grupo
   IF LASTKEY() = 27
      RETURN .F.
   ENDIF
ELSE
	IF !SEEK(m.codigo)
		wp_msg = 'Grupo nao Cadastrado'
	    RETURN .F.
	ENDIF
ENDIF
SHOW GET grup_cms.descricao

RETURN(.T.)




