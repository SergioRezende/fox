	SELE &wl_arqtmp
	SET ORDER TO TAG itensnota
	SET RELATION TO STR(empresa,3)+STR(nota,7) INTO nota ADDITIVE
	GO TOP
************************>

    LSrel = "REL611"      && relatorio padrao
    LSorienta = " WHILE    LFsegue "

