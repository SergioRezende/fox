#REGION 1
PROCEDURE ULfecha
	IF LFcbc605   	&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("cbcomiss")
	ENDIF
	IF LFitc605   	&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("itcomiss")
	ENDIF
	IF LFusr605   	&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("usuraio")
	ENDIF
	wp_flgfecha = .F. 		&& defaut nao fechamento da secao
	IF !EMPTY(LS605areant)
		SELECT &LS605areant
	ENDIF
	ON KEY LABEL ESCAPE
	SET FORMAT TO
RETURN

