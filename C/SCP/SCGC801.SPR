FUNCTION _2lx0oyrzz     &&  m.codigo VALID
#REGION 1
SELECT grupo
SET ORDER TO TAG codigo
IF LASTKEY() = 9
    ON KEY LABEL ESCAPE
	DO loc_dlog WITH .T.
    ON KEY LABEL ESCAPE DO BTN_VAL WITH 'DELETE'
	IF LASTKEY() <> 27
