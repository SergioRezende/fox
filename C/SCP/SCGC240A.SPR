FUNCTION ULatualiza
	REPLACE &wl_arqtmp .marcador WITH ""
	SCATTER MEMVAR
	SHOW GET M.DESC_T
	SHOW GET m.desc_1
	SHOW GET m.desc_2
	SHOW GET m.desc_3
	SHOW GET m.desc_4
	SHOW GET m.desc_5
	SHOW GET m.desc_6
	SHOW GET m.desc_t
	SHOW GET m.IVA
	SHOW GET LNSliq
 	SHOW GET LNSipi
 	SHOW GET LNSicmr
 	SHOW GET LNStot

	IF m.aliq_ipi = 3
		SHOW GET m.aliq_ipi COLOR W/R,GR+/R
		SET COLOR OF SCHEME 10 TO ,,,,,,GR+/R
	else
		SHOW GET m.aliq_ipi COLOR W/B,W+/B
		SET COLOR OF SCHEME 10 TO ,,,,,,W+/B
	ENDIF
RETURN(.T.)

