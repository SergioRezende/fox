CLEAR TYPEAHEAD
IF LFjanela = 3
		BROWSE  FIELDS ;
				ord=STR(recno(),2)  :H="Nr" :P="99" ;
					:W=ULinfcamp("ORD","W") AND .F.;
					:R,;
				MARCADOR :H=" " ;
