				CODFORN		:H="FORN";
					:P="@r 99999";
					:W=ULinfcamp("FORNECEDOR","W");
					:V=ULinfcamp("FORNECEDOR","V");
					:F,;
				CODIGO		:H="CODIGO";
					:P=masc_codi;
					:W=ULinfcamp("CODIGO","W");
					:V=ULinfcamp("CODIGO","V");
					:F,;
				EMPRESA 		:H="Filial";
					:P="@r 999";
					:W=ULinfcamp("FILIAL","W");
					:V=ULinfcamp("FILIAL","V");
					:F,;
				QTDE		:H="QTD/PD";
					:P="@r 99,999";
					:W=ULinfcamp("QTDE","W");
					:V=ULinfcamp("QTDE","V");
					:F,;
				QTDETR		:H="TRANS";
					:P="@r 99,999";
					:W=ULinfcamp("QTDETR","W");
					:F,;
				QTDEATD		:H="FAT";
