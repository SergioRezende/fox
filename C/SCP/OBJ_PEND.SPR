#REGION 1
PROCEDURE ULfechaNETR
	=UP_fecha("&wl_arqtmp")
	wl_arqtmp = LS_NETRarqtmp
***
	IF LFpdiNETR	&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("pedite")
	ENDIF
	IF LFgrpNETR	&& .t. =>  DEVE SER FECHADA NA SAIDA
		=UP_fecha("grupo")
	ENDIF
	LF_NETRfecha = .f. 		&& defaut nao fechamento da secao
