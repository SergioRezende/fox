BROWSE  FIELDS ;
		DATA		:H="Data";
					:R,;
		PD=STR(PEDIDO,6)		:H="Pedido";
					:P="@R 9999/99";
					:R,;
		CODIGO		:H="Codigo";
					:P=masc_codi;
					:R,;
		QTDE		:H="Qt.Ped";
					:P="@r 9999";
					:R,;
		QTDETR		:H="Qt.Tra";
					:P="@r 9999";
					:R,;
		QTDEATD		:H="Qt.Atd";
					:P="@r 9999";
					:R,;
		GRUPO.DESCRICAO	:H="Descricao";
					:R;
		TITLE LStit ;
				COLOR SCHEME 10  NODELETE NOAPPEND  NORMAL WINDOW NETR
