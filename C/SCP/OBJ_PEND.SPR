	LFgrpNETR	= .T.
	IF ! NetUse("grupo")
		LF_NETRfecha = .t.  && implica no fechamento da secao
	ENDIF
ENDIF
IF !USED("pedite")
	LFnteNETR	= .T.
	IF ! NetUse("pedite")
		LF_NETRfecha = .t.  && implica no fechamento da secao
	ENDIF
ENDIF

wl_arqtmp = ""
