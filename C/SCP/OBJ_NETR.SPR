#REGION 1
PROCEDURE ULfechaNETR
    =up_fecha("&ALS_notaent")
	=UP_fecha("&wl_arqtmp")
	IF !EMPTY(LSalias) AND USED(LSalias)
		SELECT &LSalias
	ENDIF

	POP KEY	
