#REGION 1
IF PrmProcesso = 1		
	LStit = " [ RELACAO DE NOTAS X PEDIDO ] "
ELSE
	LStit = " [ RELACAO DE NOTAS EM TRANSITO RESERVAS ] "
ENDIF
SELECT &wl_arqtmp
BROWSE  FIELDS ;
		REFERENCIA	:H="BOLETIM";
					:P="@r 999,999";
					:R,;
		DATA		:H="DT.REG";
					:R,;
		NOTA		:H="N.F.";
					:P="@r 999,999";
					:R,;
		DATA_EMI	:H="DT.EMI";
					:R,;
		CODFORN		:H="DIAS";
					:P="@r 9999";
					:R,;
		TOTAL_NOTA	:H="VLR.NOTA";
					:P="@r 999,999";
					:R,;
		PED=STR(PEDIDO,6)	:H="PEDIDO";
					:P="@r 9999/99";
					:R,;
		FIL=UPdadosemp(EMPRESA,"SIGLA",.F.)	:H="FIL";
					:R,;
		SITUACAO	:H="STT";
					:R,;
		NOME		:H="FORNECEDOR  ";
					:R;
		TITLE LStit ;
				COLOR SCHEME 10  NODELETE NOAPPEND  NORMAL WINDOW NETR
*---
RELEASE WINDOW NETR

DO ULfechaNETR
POP MENU _MSYSMENU
SET SYSMENU OFF


RETURN

