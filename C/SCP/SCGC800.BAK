FUNCTION ULRgDura
PARAMETERS PrmSaldo,PrmGiro
Private LNDura
	DO CASE
		CASE PrmGiro = 0
			IF PrmSaldo > 0
				LNDura   = 999
			ELSE
				LNDura   = 0
			ENDIF		
		OTHERWISE
			LNDura   = PrmSaldo / PrmGiro
			IF LNDura > 999
				LNDura   = 999
			ENDIF				
	ENDCASE
RETURN(LNDura)


