PROCEDURE ULvoltacampo
        LCAMPO  = VARREAD()
		IF UPPER(VARREAD()) = 'CODIGO'
			IF !BOF()
				SKIP -1
			ENDIF
			IF BOF()
			   IF !(left(situacao,1) $ "AC")
                 IF !(notaent.informaiva $ "O")
					ON KEY LABEL ESCAPE DO ULvoltacampo
					KEYBOARD CHR(27)
				   RETURN .T.
                 ENDIF

			   ENDIF
			   IF fox_alert('Abandonar Alteracoes nos itens ?')
					ON KEY LABEL ESCAPE
					KEYBOARD CHR(27)
				   RETURN .T.
			   ENDIF
			   GO TOP
			ENDIF					
		ELSE
			KEYBOARD "{BACKTAB}"
		ENDIF
RETURN

