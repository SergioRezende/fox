FUNCTION ULverqtde
	IF LASTKEY() = -3 OR LASTKEY() = 27
		CLEAR TYPEAHEAD
		RETURN .T.
	ENDIF


	REPLACE &AlsItemtmp .aliq_rdu   WITH  notaent.reduc_icms


	IF notaent.pedido = 0
		CLEAR TYPEAHEAD
		RETURN .T.
	ENDIF

	LNpendencia = pedite.qtde - pedite.qtdetr - pedite.qtdeatd
	IF &AlsItemtmp .qtde > LNpendencia + LIold_qtde
	   wp_msg =  "Quantidade superior a Pendencia => "  + ;
			   			  STR(LNpendencia,6)
			   RETURN .F.
    ENDIF

    SELE &AlsItemtmp


RETURN .T.

