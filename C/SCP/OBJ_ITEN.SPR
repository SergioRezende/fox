FUNCTION ULtipoprod  && VERIFICA SE O PRODUTO PODE SER INCL. NA OPERACAO
	SELE grfiscal
	SET ORDER TO TAG tributacao
	SEEK empresa.estado+LEFT(grupo.classifica,5)

	IF CHRTRAN(STR(grfiscal.tp_mercad,1)," ","-")  $ tipooper.tipo_produ
