PROCEDURE ULIMPRIME
	SELECT  retornmv
	SET ORDER TO TAG r_ocorr
	SET NEAR ON
	SEEK STR(m.empresa,3)+STR(m.banco,3)+;
	    m.carteira+m.variacao+STR(m.aviso,8)
