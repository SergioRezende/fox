			ON ERROR DO ERRO
			LDdtinicio = wp_dtoper
			DO OBJ_DAT2.SPR WITH LDdtinicio
			m.dtaviso  	 = LDdtinicio
			m.dtprocesso = LDdtinicio

			IF !fox_alert('Confirma Importacao do Caixa de '+;
					DTOC(m.dtaviso)+' ?')
