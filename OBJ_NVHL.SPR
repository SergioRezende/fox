*       є                                                         є
*       є _RX20T0N1W           m.menu VALID                       є
*       є                                                         є
*       є Function Origin:                                        є
*       є                                                         є
*       є From Platform:       MS-DOS                             є
*       є From Screen:         HLP2000,     Record Number:    3   є
*       є Variable:            m.menu                             є
*       є Called By:           VALID Clause                       є
*       є Object Type:         Field                              є
*       є Snippet Number:      1                                  є
*       є                                                         є
*       УДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДЅ
*
FUNCTION _rx20t0n1w     &&  m.menu VALID
#REGION 2
IF LASTKEY() = 27
	IF isediting
	    ON KEY LABEL ESCAPE DO BTN_VAL WITH 'DELETE'
	ELSE
    	ON KEY LABEL ESCAPE DO BTN_VAL1 WITH 'BUSCA'
	ENDIF
ENDIF	


SELE hlp
SET ORDER TO TAG loc_auto
IF LASTKEY() = 9
    ON KEY LABEL ESCAPE
	DO loc_dlog WITH .F., "EMPTY(ALLTIM(BARRA))", ;
						  "m.menu+SPACE(43)"
	IF isediting
	    ON KEY LABEL ESCAPE DO BTN_VAL WITH 'DELETE'
	ELSE
    	ON KEY LABEL ESCAPE DO BTN_VAL1 WITH 'BUSCA'
	ENDIF
	IF LASTKEY() <> 27
		wp_msg = ''
		RETURN .F.
	ENDIF	
	m.menu = hlp.menu
ENDIF	
IF isadding
	RETURN .T.
ENDIF
SEEK m.menu+space(43)

IF !FOUND()
	IF isadding
		RETURN .T.
	ENDIF
	wp_msg = 'Menu nao possui ajuda.'
	RETURN .F.
ENDIF	
SCATTER MEMVAR MEMO
SHOW GETS
RETURN .T.




