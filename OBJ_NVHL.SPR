ON KEY LABEL ESCAPE KEYBOARD "{TAB}"


@ 4,1 EDIT m.ajuda ;
	SIZE 13,74,0 ;
	DEFAULT " " ;
	NOMODIF;
	SCROLL
READ

ON KEY LABEL ENTER
ON KEY LABEL ESCAPE

RETURN


